alloc(UnflipPlayers,$100)
createthread(UnflipPlayers)

UnflipPlayers:
  push edi
  push esi
  mov edi,-1
  @@:
  inc edi
  cmp edi,#20
  jae @f
  imul esi,edi,260
  add esi,car_data
  cmp byte ptr [esi+14D],1
  jne @b
  push 00
  push esi
  call TempBuildHandlingMatrix
  add esp,4*2
  jmp @b
  @@:
  pop esi
  pop edi
  ret


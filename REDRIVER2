          <AssemblerScript>[ENABLE]
alloc(RepairPlayers,$80)
createthread(RepairPlayers)

RepairPlayers:
  push esi
  push edi
  mov edi,-1
  @@:
  inc edi
  cmp edi,#20
  jae @f
  imul esi,edi,260
  add esi,car_data
  cmp byte ptr [esi+14D],1
  jne @b
  //Reset damage level:
  mov word ptr [esi+25A],0
  //Reset visual damage:
  mov [esi+13E],0
  mov [esi+13E+4],0
  mov [esi+13E+8],0
  //Reset hubcaps:
  mov byte ptr [esi+13D],0
  push esi
  call DentCar
  add esp,04
  jmp @b
  @@:
  pop edi
  pop esi
  ret

{$lua}
if syntaxcheck then return end

local t=createTimer()
t.Interval=50
t.OnTimer=function()
  memrec.Active=false
  t.destroy()
end

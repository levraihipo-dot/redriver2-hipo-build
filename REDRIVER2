        <CheatEntry>
          <ID>1338</ID>
          <Description>"Unreleased/Discontinued"</Description>
          <Options moHideChildren="1"/>
          <LastState Value="" RealAddress="00000000"/>
          <Color>808080</Color>
          <GroupHeader>1</GroupHeader>
          <CheatEntries>
            <CheatEntry>
              <ID>1266</ID>
              <Description>"Players Objectives (WIP)"</Description>
              <LastState/>
              <Color>0000FF</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>{ Game   : REDRIVER2.exe
  Version: 
  Date   : 2023-06-10
  Author : samue

  This script does blah blah blah
}

[ENABLE]

{Custom flags:
  - TARGET_FLAG_INDIVIDUAL = 0x2000
  - TARGET_FLAG_ORTARGET = 0x4000
  - TARGET_FLAG_CAR_DELIVERY = 0x8000
}

aobscanmodule(SetCoopObjectives,$process,89 01 B9 01 00 00 00) // should be unique
alloc(newmem,$200)

label(code)
label(return)

newmem:

code:
  cmp byte ptr [NumPlayers],1
  je originalObj
  push edi //Player 1 objective address.
  push edx //Player 1 objective ID.
  push ecx //Player 2 objective address.
  mov edx,[esi+0C]
  mov edx,[edx]
  mov edi,edx
  shl edi,6
  add edi,[MissionTargets]
  mov ecx,[MissionHeader]
  add ecx,84
  add ecx,380 //p2 uses waypoint E.

  //Missions data:
  {    Failed attempts for p2 objectives (idk how threads work, fuck it)
  Mission1:
  cmp [gCurrentMissionNumber],#1 //Surveillance tip off:
  jne Mission2
  cmp edx,0 //First objective.
  jne @f
  cmp byte ptr [car_data+1560+14D],0
  je @f
  or [edi+4],2000 //Individual (P1) objective.
  mov byte ptr [ecx],2 //P2 objective: Vehicle.
  or [ecx+4],40002C00 //0x2000 = Individual + 0xC00 = P2 objective.
  or [ecx+8],20 //Activate markers.
  mov [ecx+14],00004000
  mov [ecx+18],9 //Vehicle ID.
  or [ecx+28],30
  mov [ecx+34],4E20 //Car max damage to fail mission.
  call ProcessSeparateTargets
  jmp finishObjectiveProcess
  @@:

  jmp finishObjectiveProcess}
  Mission11:
  cmp [gCurrentMissionNumber],#11
  jne Mission12
  cmp edx,#4
  jne @f
  or [edi+4],8000 //delivery.
  @@:
  jmp ObjectiveProcess

  Mission12:

  ObjectiveProcess:
  //Make threads work for both players (workaround):
  test [edi+4],8000
  jne finishObjectiveProcess
  cmp eax,0
  jne @f
  xor [esi+1],1
  @@: //Check if there is target:
  push eax
  mov eax,[player+43]
  mov [player+43+74],al //Make it p2 target too.
  pop eax
  cmp byte ptr [player+43],-01
  je theresNoTarget
  push eax
  push ecx
  push edi
  push ebx
  push edx
  mov ebx,car_data+14
  movzx edi,byte ptr [player+43]
  imul edi,260
  add ebx,edi
  push ebx
  push player
  call Long2DDistance
  mov edi,eax
  add esp,08
  push ebx
  push player+74
  call Long2DDistance
  add esp,08
  pop edx
  pop ebx
  cmp edi,eax
  pop edi
  pop ecx
  pop eax
  jl @f
  mov byte ptr [MissionThreads+01],01 //Use P2 distance.
  mov byte ptr [MissionThreads+11],01
  mov byte ptr [MissionThreads+21],01
  mov byte ptr [MissionThreads+31],01
  mov byte ptr [MissionThreads+41],01
  jmp theresNoTarget
  @@:
  mov byte ptr [MissionThreads+01],00 //Use P1 distance.
  mov byte ptr [MissionThreads+11],00
  mov byte ptr [MissionThreads+21],00
  mov byte ptr [MissionThreads+31],00
  mov byte ptr [MissionThreads+41],00
  theresNoTarget:

  //Make markers be shared between players (workaround):
  test byte ptr [edi+4],1
  je finishObjectiveProcess
  or byte ptr [edi+5],8

  finishObjectiveProcess:
  test byte ptr [edi+4],2
  jne @f
  test byte ptr [edi+5],1
  je continue34
  @@:
  or byte ptr [edi+4],02 //p1 complete.
  or byte ptr [edi+5],01 //p2 complete.
  continue34:
  pop ecx
  pop edx
  pop edi

  originalObj:
  mov [ecx],eax
  mov ecx,00000001
  jmp return

  //Calls:
  {  Unused call for now until I figure out how threads work.
  ProcessSeparateTargets: //input: eax=p1completed, esi, edi, edx; output eax=allcompleted.
  test [edi+4],2000
  jne @f
  xor [esi+1],1
  @@:
  cmp eax,0
  je @f
  test [edi+4],4000
  je @f
  ret
  @@:
  push ecx
  push edx
  lea edx,[MissionThreads+C0]
  mov word ptr [edx],0101
  push ecx
  push edx
  call MRProcessTarget
  add esp,8
  pop edx
  pop ecx
  ret}

SetCoopObjectives:
  jmp newmem
  nop 2
return:
registersymbol(SetCoopObjectives)

[DISABLE]

SetCoopObjectives:
  db 89 01 B9 01 00 00 00

unregistersymbol(SetCoopObjectives)
dealloc(newmem)

{

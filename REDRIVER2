</AssemblerScript>
          <CheatEntries>
            <CheatEntry>
              <ID>1681</ID>
              <Description>"Reset Vehicle List"</Description>
              <Options moDeactivateChildrenAsWell="1"/>
              <LastState/>
              <Color>C08000</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>{$LUA}

[ENABLE]

if syntaxcheck then return end

local t=createTimer()
t.Interval=50
t.OnTimer=function()
  memrec.Active=false
  t.destroy()
end

local carModelSlot = getAddressSafe("CarSlot")
local city = getAddressSafe("GameLevel")
local gamemode = getAddressSafe("GameType")

if not carModelSlot or not city or not gamemode then return end

if readInteger(city) == 2 and readInteger(gamemode) == 1 then
  writeInteger(carModelSlot, 1)
  writeInteger(carModelSlot+0x4, 3)
  writeInteger(carModelSlot+0x8, 4)
  writeInteger(carModelSlot+0xC, 0)
  writeInteger(carModelSlot+0x10, -1)
else
  writeInteger(carModelSlot, 1)
  writeInteger(carModelSlot+0x4, 2)
  writeInteger(carModelSlot+0x8, 3)
  writeInteger(carModelSlot+0xC, 0)
  writeInteger(carModelSlot+0x10, -1)
end


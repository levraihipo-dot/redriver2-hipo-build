aobscanmodule(FlashMinimap,$process,B9 92 02 00 00 66 39 08 0F 8E C4) // should be unique
alloc(newmemm,$100)
//16
//Flash minimap when p2 is being chased.
label(codem)
label(returnm)

newmemm:

codem:
  cmp byte ptr [NumPlayers],01
  je lfyo
  cmp [numActiveCops],00 //Fix flash when innocent player enters pursuer FOV.
  jne @f
  cmp [numRoadblockCars],00
  jne @f
  xor eax,eax
  cmp eax,00
  jmp returnm
  @@:
  mov ecx,00000292
  cmp [eax],cx
  jg returnm+6
  movsx eax,[player+48+74]
  cmp eax,-1
  jne @f
  xor eax,eax
  cmp ax,cx
  jmp returnm
  @@:
  imul eax,260
  add eax,car_data+252
  cmp [eax],cx
  jg returnm+6
  jmp returnm
  lfyo:
  mov ecx,292
  cmp [eax],cx
  jmp returnm

FlashMinimap:
  jmp newmemm
  nop 3
returnm:
registersymbol(FlashMinimap)


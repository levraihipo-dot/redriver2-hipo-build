            <CheatEntry>
              <ID>1173</ID>
              <Description>"Day Cycle (Old Attempt)"</Description>
              <LastState/>
              <Color>0000FF</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]
alloc(DayCycleData,$10)
registersymbol(DayCycleData)
 {-00 w: Clock.
  -02: Steps to +1 minute.}

aobscanmodule(DayCycle,$process,83 C4 04 84 C0 75 31) // should be unique
alloc(newmem,$280)

label(code)
label(return)

newmem:

code:
  add esp,04

  Cycle:
  push eax
  push ebx
  push ecx
  push edx
  push edi
  push esi
  //Control Clock: -----------------------------
  //Init:
  cmp [CameraCnt],1
  jne CountClock
  call LoadSky
  cmp [gTimeOfDay],0
  jne @f
  mov word ptr [DayCycleData],#301
  @@:
  cmp [gTimeOfDay],1
  jne @f
  mov word ptr [DayCycleData],#720
  @@:
  cmp [gTimeOfDay],2
  jne @f
  mov word ptr [DayCycleData],#1021
  @@:
  cmp [gTimeOfDay],3
  jne @f
  mov word ptr [DayCycleData],#1201
  @@:
  //Count clock:
  CountClock:
  inc byte ptr [DayCycleData+2]
  cmp byte ptr [DayCycleData+2],#30 //30 steps = 1 minute.
  jne @f
  mov byte ptr [DayCycleData+2],0
  inc word ptr [DayCycleData]
  @@:
  //Reset 24h:
  cmp word ptr [DayCycleData],#1440
  jb @f
  mov word ptr [DayCycleData],0
  @@:
  //Set up environment: ---------------------------
  cmp word ptr [DayCycleData],#300 //Dawn (5am).
  jne @f
  cmp byte ptr [DayCycleData+2],0
  jne @f
  mov [DawnCount],0
  mov [gTimeOfDay],0
  mov [gNight],0
  call LoadSky
  @@:
  cmp word ptr [DayCycleData],#600 //Day (10am).
  jne @f
  cmp byte ptr [DayCycleData+2],0
  jne @f
  mov [gTimeOfDay],1
  mov [gNight],0
  call LoadSky
  @@:
  cmp word ptr [DayCycleData],#1020 //Dusk (5pm).
  jne @f
  cmp byte ptr [DayCycleData+2],0
  jne @f
  mov [DawnCount],#-1000
  mov [gTimeOfDay],2
  mov [gNight],0
  call LoadSky
  @@:
  cmp word ptr [DayCycleData],#1200 //Night (8pm).
  jne @f
  cmp byte ptr [DayCycleData+2],0
  jne @f
  mov [gTimeOfDay],3
  mov [gNight],1
  call ReloadLevel
  call LoadSky
  @@:
  pop esi
  pop edi
  pop edx
  pop ecx
  pop ebx
  pop eax

  test al,al
  jmp return

  ReloadLevel:
mov [seated_pedestrian],0
push [GameLevel]
call LoadCosmetics
add esp,4
xor eax,eax
cmp [gMultiplayerLevels],eax
jne @f
cmp dword ptr [gTimeOfDay],03
sete al
jmp sdfjh
@@:
cmp dword ptr [gTimeOfDay],03
sete al
add eax,02
sdfjh:
push eax
call SetCityType
mov ecx,[GameLevel]
shl ecx,05
mov eax,[ecx+citylumps]
cdq
and edx,000007FF
lea esi,[edx+eax]
mov eax,[ecx+citylumps+4]
cdq
and edx,000007FF
sar esi,0B
mov [ebp-108],esi
lea edi,[edx+eax]
sar edi,0B
push edi
push esi
push [_primTab1]
call loadsectors
add [ebp-108],edi
mov eax,[_primTab1]
shl edi,0B
add eax,08
push edi
push eax
call ProcessLumps
lea eax,[ebp-108]
push eax
//call LoadPermanentTPages
mov ecx,[GameLevel]
mov edi,[mallocptr]
shl ecx,05
mov eax,[ecx+citylumps+10]
cdq
and edx,000007FF
add eax,edx
sar eax,0B
mov [ebp-10C],eax
mov [ebp-108],eax
mov eax,[ecx+citylumps+14]
cdq
and edx,000007FF
lea ebx,[edx+eax]
sar ebx,0B
mov esi,ebx
shl esi,0B
push ebx
push [ebp-10C]
lea ecx,[esi+edi]
push edi
mov [mallocptr],ecx
call loadsectors
add [ebp-108],ebx
lea eax,[edi+08]
push esi
push eax
call ProcessLumps
mov eax,[GameLevel]
add esp,C*4
shl eax,05
mov eax,[eax+citylumps+18]
mov [SpoolLumpOffset],eax
call InitDebrisNames
call InitShadow
cmp dword ptr [gContentOverride],00
je @f
call LoadPermanentTPagesFromTIM
@@:
mov [IconsLoaded],1
push 0
call LoadFont
add esp,4
call InitSpooling
call InitMap
  {push 0
  call State_GameInit
  add esp,4}
  ret

DayCycle:
  jmp newmem
return:
registersymbol(DayCycle)

[DISABLE]

DayCycle:
  db 83 C4 04 84 C0

unregistersymbol(DayCycle)
dealloc(newmem)

unregistersymbol(DayCycleData)
dealloc(DayCycleData)


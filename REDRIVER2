            <CheatEntry>
              <ID>1391</ID>
              <Description>"HUD Scale Test"</Description>
              <Options moHideChildren="1"/>
              <LastState/>
              <Color>0000FF</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]

alloc(OverlayScale,$10)
registersymbol(OverlayScale)
{- 00 float: scale multiplier.
 - 04: HUD position multiplier.
 - 05: HUD scale percentage (decimal).
}

OverlayScale:
  dd 3F800000

alloc(OverlaysXY,$100)
registersymbol(OverlaysXY)
{- 00-7F: saved coords
 - 80-FF: saved addresses
}

OverlaysXY+80:
  dd gMapXOffset
  dd gMapYOffset
  dd PlayerDamageBar+6
  dd PlayerDamageBar+4
  dd Player2DamageBar+6
  dd Player2DamageBar+4
  dd DamageBar+6
  dd DamageBar+4
  dd FelonyBar+6
  dd FelonyBar+4
  dd ProxyBar+6
  dd ProxyBar+4
  dd Mission+1C
  dd Mission+1E
  dd Mission+28
  dd Mission+2A
  dd -1

aobscanmodule(ScaleHUD,$process,F3 0F 11 4D 08) // should be unique
alloc(newmem,$100)

label(code)
label(return)

newmem:

code:
  cmp [gInFrontend],0
  jne DontScaleHUD
  mulss xmm1,[OverlayScale]

  {push eax
  mov eax,[gMapXOffset]
  cmp [OverlaysXY],eax
  je @f
  imul eax,2
  mov [gMapXOffset],eax
  mov [OverlaysXY],eax
  @@:
  pop eax}

  push eax
  push esi
  push ecx
  push edx
  mov ecx,-1
  @@:
  inc ecx
  imul edx,ecx,4
  mov esi,[edx+OverlaysXY+80]
  cmp esi,-1
  je @f
  movsx eax,word ptr [esi]
  cmp eax,[edx+OverlaysXY]
  je @b
  push ecx
  movzx ecx,byte ptr [OverlayScale+4]
    //mov ecx,#32
  imul eax,ecx
  sar eax,4
  pop ecx
  //imul eax,2
  mov [esi],ax
  mov [edx+OverlaysXY],eax
  jmp @b
  @@:
  pop edx
  pop ecx
  pop esi
  pop eax

  DontScaleHUD:
  movss [ebp+08],xmm1
  jmp return

ScaleHUD:
  jmp newmem
return:
registersymbol(ScaleHUD)

[DISABLE]

ScaleHUD:
  db F3 0F 11 4D 08

unregistersymbol(ScaleHUD)
dealloc(newmem)

unregistersymbol(OverlayScale)
dealloc(OverlayScale)

unregistersymbol(OverlaysXY)
dealloc(OverlaysXY)


aobscanmodule(UpdateRoadblockCopTarget,$process,66 C7 86 4D 01 00 00 03 00) // should be unique
alloc(newmemu,$100)
//27
label(codeu)
label(returnu)

newmemu:

codeu:
  mov word ptr [esi+0000014D],0003

  cmp byte ptr [NumPlayers],01
  jne ChaseWhoHitIt
  push eax
  mov eax,[player+48]
  mov [esi+14B],al
  pop eax
  jmp returnu

  ChaseWhoHitIt:
  push eax
  push ecx
  push edx
  push edi
  lea ecx,[esi+14]
  push player
  push ecx
  call Long2DDistance
  add esp,08
  mov edi,eax
  lea ecx,[esi+14]
  push player+74
  push ecx
  call Long2DDistance
  add esp,08
  xor ecx,ecx
  cmp edi,eax
  jb @f
  mov ecx,74
  @@:
  mov ecx,[ecx+player+48]
  mov [esi+14B],cl
  pop edi
  pop edx
  pop ecx
  pop eax
  jmp returnu

UpdateRoadblockCopTarget:
  jmp newmemu
  nop 4
returnu:
registersymbol(UpdateRoadblockCopTarget)


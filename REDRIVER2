aobscanmodule(MPEmergHornFix,$process,80 7E 1C 00 6A 00) // should be unique
alloc(newmema,$80)

label(codea)
label(returna)

newmema:

codea:
  push eax
  imul eax,ebx,74
  add eax,channels
  test byte ptr [eax+72],01 //Checks if sound loops.
  jne OriginalCode
  //Stop horn when release button:
  test byte ptr [edi+0000025C],08
  jne @f
  mov [eax+58],00
  jmp NoLoop
  @@:
  //Loop horn:
  cmp byte ptr [eax+70],02
  jne NoLoop
  push eax
  mov eax,00
  cmp ebx,05
  jne @f
  mov eax,74
  @@:
  mov [eax+player+62],01
  pop eax
  NoLoop:
  pop eax
  jmp SoundTasks+1EF

  OriginalCode:
  pop eax

  cmp byte ptr [esi+1C],00 //Original Code
  push 00
  jmp returna

MPEmergHornFix-2:
  nop 2
  jmp newmema
  nop
returna:
registersymbol(MPEmergHornFix)


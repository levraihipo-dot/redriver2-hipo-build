aobscanmodule(SightConeSize,$process,B9 92 02 00 00 66 39 08 7E ** B8 00 04 00 00) // should be unique
alloc(newmeml,$100)
//15
//When p2 has felony level draw big cone.
label(codel)
label(returnl)

newmeml:

codel:
  mov ecx,00000292
  push eax
  push ecx
  movsx ecx,word ptr [pedestrianFelony]
  movzx eax,byte ptr [PlayerNum]
  imul eax,74
  movsx eax,[eax+player+48]
  cmp eax,-1
  jle @f
  imul eax,260
  movsx ecx,word ptr [eax+car_data+252]
  @@:
  cmp ecx,252
  pop ecx
  pop eax
  jbe putSmallCone
  jmp putBigCone

  putSmallCone:
  push eax
  mov eax,0
  cmp eax,ecx
  pop eax
  jmp returnl

  putBigCone:
  jg returnl+2
  jmp returnl

SightConeSize:
  jmp newmeml
  nop 3
returnl:
registersymbol(SightConeSize)


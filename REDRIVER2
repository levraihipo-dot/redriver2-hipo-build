if LUACodeScriptTimer == nil then
  LUACodeScriptTimer = createTimer(nil)
  LUACodeScriptTimer.Interval = 500
  LUACodeScriptTimer.OnTimer = function()
  --Check the scripts by name:
  local script1 = getAddressList().getMemoryRecordByDescription'Better Multiplayer Menus'
  if script1 and script1.active == true then
    doCoopVersus()
  end
  local script2 = getAddressList().getMemoryRecordByDescription'Improved Vehicle Selection'
  if script2 and script2.active == true then
    doVehicleSelectionProcedures()
  end

  --The scripts functions:
  function doCoopVersus()
    local versus = getAddressList().getMemoryRecordByDescription'Versus Quick Getaway and Survival'
    local MPVersus = getAddressSafe("MPVersus")
    local gameType = getAddressSafe("GameType")
    --print("crap")
    if not MPVersus or not gameType then return end
    if readInteger(MPVersus) == 0 and versus.active == true then
      versus.active = false
    elseif readInteger(MPVersus) ~= 0 and versus.active == false then
      versus.active = true
    end
  end


  function doVehicleSelectionProcedures()
    local missionHeader = getAddressSafe("MissionHeader")
    local civCarBase = getAddressSafe("PingInCivCar")
    local gameType = getAddressSafe("GameType")
    local cameraCnt = getAddressSafe("cameraCnt")
    local copSpeech = getAddressSafe("gDoCopSpeech")
    local felonyBase = getAddressSafe("FelonyBar")
    local city = getAddressSafe("GameLevel")
    local LoadSFXFunction = getAddressSafe("LoadLevelSFX")
    local pursueCiv = getAddressSafe("PursueCiv")
    --print("shit")

    if not civCarBase
      or not gameType
      or not cameraCnt
      or not copSpeech
      or not city
      or not LoadSFXFunction
      or not pursueCiv
      or not felonyBase then return end

    local civCarRandom = civCarBase + 0x566
    local felonyHide = felonyBase + 0x14
    local LoadSFXCopEngine = LoadSFXFunction + 0x45
    local civCarCopModel = civCarBase + 0x587

    if readInteger(city) == 2 and readInteger(gameType) == 1 then --correct cop engine
      writeByte(LoadSFXCopEngine,3)
    else
      writeByte(LoadSFXCopEngine,2)
    end

    if not missionHeader
      or readInteger(missionHeader) == 0
      or not readInteger(missionHeader) then return end

    if readInteger(gameType) ~= 0 then
      local carSlot2 = readInteger(readInteger(missionHeader) + 0x68)
      if not carSlot2 then return end
      if carSlot2 &lt; 1 or carSlot2 &gt; 4 then
        writeInteger(civCarRandom,0x01010000) --don't spawn cop car.
      else
        writeInteger(civCarRandom,0x02020202)
      end
      local copSlot = readInteger(readInteger(missionHeader) + 0x60 + readInteger(civCarCopModel) * 4) --disable cop speech and felony
      if readInteger(cameraCnt) &lt;= 20 and  copSlot ~= 0 then
        writeInteger(copSpeech,0)
        writeInteger(felonyHide,2)
      end
    end
    --Spawn cops even when pursuers are gangsters:
    --if readInteger(readInteger(missionHeader) + 0x68) == 0
    --and readInteger(readInteger(missionHeader) + 0x6C) ~= 0
    --and readInteger(gameType) == 1
    --and readByte(pursueCiv) ~= 0
    --and readByte(pursueCiv + 0x2) ~= 0
    --then
    --  if readInteger(civCarCopModel) == 3 then
    --    writeInteger(civCarCopModel,2)
    --  else
    --    writeInteger(civCarCopModel,3)
    --  end
    --else
    --  writeInteger(civCarCopModel,3)
    --end
  end



  end
end


            <CheatEntry>
              <ID>1272</ID>
              <Description>"Switch Single Player/Multiplayer"</Description>
              <LastState/>
              <Color>0000FF</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]
alloc(SwitchPlayerNumber,$400)
createthread(SwitchPlayerNumber)

SwitchPlayerNumber:
  cmp byte ptr [NumPlayers],1
  jne toSinglePlayer
  push eax
  push ebx
  push ecx
  push edx
  push esi
  mov byte ptr [Player2DamageBar+18],1
  mov byte ptr [NumPlayers],2
  call InitModdedHUD
  //Spawn player 2:
  push 0 //Extra data.
  push 0 //Palette.
  mov eax,1
  movsx ecx,byte ptr [player+48] //same model as p1.
  cmp ecx,0
  jl @f
  imul ecx,260
  movzx eax,byte ptr [ecx+car_data+13B]
  @@:
  push eax //Model.
  push 1 //Control.
  push player //Position.
  mov eax,[player+10]
  //sub eax,400
  push eax //Direction.
  mov eax,-1 //i.
  @@: //Get vehicle slot:
  inc eax
  imul esi,eax,260
  add esi,car_data
  cmp eax,#19 //i &lt; 19
  jae @f
  cmp byte ptr [esi+0000014D],00
  jne @b
  @@:
  mov [player+48+74],al
  mov [player+49+74],al
  push esi //Car slot.
  call InitCar
  add esp,4*7
  mov [esi+150],player+BE
  mov byte ptr [player+BE],1
  mov eax,[player+10]
  sub eax,400
  and eax,FFF
  mov ecx,[eax*4+rcossin_tbl+2]
  mov eax,[eax*4+rcossin_tbl]
  sar ecx,13
  sar eax,13
  add [esi+14],ecx
  add [esi+1C],eax
  pop esi
  pop edx
  pop ecx
  pop ebx
  pop eax
  ret

  toSinglePlayer:
  push eax
  push ebx
  push ecx
  push edx
  push esi
  mov byte ptr [NumPlayers],1
  mov byte ptr [Player2DamageBar+18],0
  call InitModdedHUD
  movsx eax,[player+48+74]
  cmp eax,0
  jl killP2Ped
  imul eax,260
  mov esi,car_data
  add esi,eax
  mov byte ptr [esi+14D],2
  push esi
  call PingOutCar
  add esp,4
  jmp finalizeSPChange
  killP2Ped:
  push [player+6C+74]
  call DestroyPedestrian
  add esp,4
  finalizeSPChange:
  pop esi
  pop edx
  pop ecx
  pop ebx
  pop eax
  ret

{$lua}
if syntaxcheck then return end

local t=createTimer()
t.Interval=50
t.OnTimer=function()
  memrec.Active=false
  t.destroy()
end
{$asm}

[DISABLE]
dealloc(SwitchPlayerNumber)

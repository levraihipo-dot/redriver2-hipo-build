vehicleSlotListTimer = createTimer()
vehicleSlotListTimer.Interval = 500
vehicleSlotListTimer.Enabled = false

function getVehicleNameMap(cityID)
    local map = {}

    local base = {
        [0] = {"Chicago Police", "Oldsmobile 442", "Monarch", "Delta", "Chicago Van"},
        [1] = {"Havana Police", "Kingsway", "Bel Air", "Styleline", "Havana Pickup"},
        [2] = {"Vegas Police", "Challenger", "Lincoln", "Cougar", "Vegas Pickup"},
        [3] = {"Rio Police", "Mercedes", "Thunderbird", "Corcel", "Ram Pickup"},
    }

    local extra = {
        [0] = {"Chicago Fire", "Armored Truck", "School Bus", "Big Rig", "Secret Chicago Car", "Extra Vehicle"},
        [1] = {"Flatbed", "Havana Truck", "Havana Bus", "Kombi", "Secret Havana Car", "Extra Vehicle"},
        [2] = {"White Limo", "Ambulance", "Vegas Truck", "SUV", "Secret Vegas Car", "Extra Vehicle"},
        [3] = {"Rio Bus", "C-Series Pickup", "Rio Fire", "Black Limo", "Secret Rio Truck", "Extra Vehicle"},
    }

    for i = 0, 4 do
        map[i] = base[cityID] and base[cityID][i+1] or "Unknown"
    end
    for i = 8, 13 do
        map[i] = extra[cityID] and extra[cityID][i-7] or "Unknown"
    end

    return map
end

function getVehicleSlotDropdown()
    local missionHeaderAddr = getAddressSafe("MissionHeader")
    local gameLevelAddr = getAddressSafe("GameLevel")
    local ctValuesAddr = getAddressSafe("CTValues")

    if not missionHeaderAddr or not gameLevelAddr or not ctValuesAddr then return "" end

    if readInteger(missionHeaderAddr) == 0 then
      return ""
    end

    local gameLevel = readInteger(gameLevelAddr)
    local altModeFlag = readBytes(ctValuesAddr + 0x12, 1)
    local altCity = readInteger(ctValuesAddr + 0x13)

    local gameMap = getVehicleNameMap(gameLevel)
    local altMap = getVehicleNameMap(altCity)

    local dropdown = ""

    for i = 0, 4 do
        local offset = 0x60 + (i * 4)
        if readInteger(missionHeaderAddr) == nil then return "" end
        local modelID = readInteger(readInteger(missionHeaderAddr) + offset)

        local name
        if i == 4 and altModeFlag == 1 then
            name = altMap[modelID] or ("[" .. modelID .. "]")
        else
            name = gameMap[modelID] or ("[" .. modelID .. "]")
        end

        dropdown = dropdown .. (i) .. ":" .. name .. "\n"
    end

    return dropdown
end

vehicleSlotListTimer.OnTimer = function(timer)
    local record = getAddressList().getMemoryRecordByDescription("VehicleSlotList")
    if record then
        record.DropDownList.Text = getVehicleSlotDropdown()
        record.DisplayAsDropDownListItem = true
        record.DropDownDescriptionOnly = true
    end
end

vehicleSlotListTimer.Enabled = true


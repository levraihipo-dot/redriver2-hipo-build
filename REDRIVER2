  jmp returno

Player2HonkFelony:
  jmp newmemo
  nop 4
returno:
registersymbol(Player2HonkFelony)

//--------------------------------------------------------------------------------

alloc(spottedByRoadblock,$10)
 {-0: Player already spotted by pursuers as felon.
  -4: Cops can see player 1.
  -5: Cops can see player 2.
  -6: Player 1 wanted.
  -7: Player 2 wanted.}

aobscanmodule(FixCopVoiceLooping,$process,83 3D ** ** ** ** 00 A1 ** ** ** ** 75 ** 3D 00 01 00 00) // should be unique
alloc(newmemp,$300)
//20
label(codep)
label(returnp)

newmemp:

codep:
  cmp byte ptr [NumPlayers],01
  je jyou

  //Checks:
  @@:
  cmp [numActiveCops],00
  je NoActivePursuersNoLoop

  ActivePursuersNoLoop:
  cmp [CopsCanSeePlayer],00
  je jyou
  mov [said_picked_up],1

  jyou:
  cmp dword ptr [numActiveCops],00
  jmp returnp

  NoActivePursuersNoLoop:
  //Check if individual players seen:
  push eax
  push ebx
  movzx eax,byte ptr [PlayerNum]
  mov ebx,[CopsCanSeePlayer]
  mov [eax+spottedByRoadblock+4],bl
  pop ebx
  pop eax
  //Check if wanted:
  push ecx
  push ebx
  mov ecx,-1
  NxkpFelony:
  inc ecx
  cmp ecx,1
  ja noMoflchck
  imul ebx,ecx,74
  movsx ebx,byte ptr [ebx+player+48]
  cmp ebx,-1
  jne @f
  movzx ebx,word ptr [pedestrianFelony]
  jmp onf36
  @@:
  imul ebx,260
  movzx ebx,word ptr [ebx+car_data+252]
  onf36:
  cmp ebx,292
  jbe @f
  mov byte ptr [ecx+spottedByRoadblock+6],01
  jmp NxkpFelony
  @@:
  mov byte ptr [ecx+spottedByRoadblock+6],00
  jmp NxkpFelony
  noMoflchck:
  pop ebx
  pop ecx

  push eax
  push ebx
  movzx eax,byte ptr [spottedByRoadblock+4]
  movzx ebx,byte ptr [spottedByRoadblock+5]
  shl ebx,1
  or eax,ebx
  movzx ebx,byte ptr [spottedByRoadblock+6]
  shl ebx,2
  or eax,ebx
  movzx ebx,byte ptr [spottedByRoadblock+7]
  shl ebx,3
  or eax,ebx
  pop ebx
  cmp al,9
  jne @f
  pop eax
  jmp pnotseen
  @@:
  cmp al,6
  jne @f
  pop eax
  jmp pnotseen
  @@:
  push eax
  and eax,C
  cmp eax,0
  pop eax
  jne @f
  pop eax
  mov byte ptr [spottedByRoadblock],00
  jmp jyou
  @@:
  push eax
  and eax,3
  cmp eax,0
  pop eax
  jne @f
  mov byte ptr [spottedByRoadblock],00
  pop eax
  jmp jyou
  @@:
  pop eax
  cmp byte ptr [spottedByRoadblock],00
  jne doTheReturn
  mov byte ptr [spottedByRoadblock],01
  jmp jyou

  pnotseen:
  cmp byte ptr [spottedByRoadblock],00
  je @f
  mov byte ptr [spottedByRoadblock],00
  jmp jyou
  @@:
  mov [player_position_known],0

  doTheReturn:
  pop edi
  pop esi
  pop ebx
  mov ecx,[ebp-04]
  xor ecx,ebp
  call __security_check_cookie
  mov esp,ebp
  pop ebp
  ret

FixCopVoiceLooping:
  jmp newmemp
  nop 2
returnp:
registersymbol(FixCopVoiceLooping)


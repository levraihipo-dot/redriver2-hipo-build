function getPlayerControlList()
    local PlayerControlList = ""
    local player = getAddressSafe("player")
    if not player then player = 0 end
    playerControl = player + 0x4A

    PlayerControlList = PlayerControlList .. tonumber(playerControl) .. ":Player 1\n"
    for i=1,15 do
      playerControl = playerControl + 0x74
      --playerPadValue = readBytes(player)
      --if playerPadValue == i then
      PlayerControlList = PlayerControlList .. tonumber(playerControl) .. ":Player " .. i+1 .. "\n"
      --else

      --end
    end
    return PlayerControlList
end


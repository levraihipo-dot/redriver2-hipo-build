aobscanmodule(SmartCopAI1,$process,C1 F9 04 C1 F8 04 0F AF C9) // should be unique
alloc(newmemqa,$100)
//21
label(codeqa)
label(returnqa)

newmemqa:

codeqa:
  push eax
  push ecx
  push edx
  call UpdateCopMap //Update playerTargetDistanceSq fast enough.
  pop edx
  pop ecx
  pop eax

  push eax
  push esi
  mov esi,[CurrentPursuer]
  call GetPursuerTarget
  pop esi
  mov ebx,eax
  pop eax
  sub ebx,car_data
mov edx,[ebp+08]
sub edx,[ebx+car_data+14]
mov esi,[ebp+10]
mov ecx,esi
sub ecx,[ebx+car_data+1C]
mov edi,[ebp+0C]
mov eax,edi
sub eax,[ebx+car_data+18]
mov ebx,[ebp+08]

  sar ecx,04
  sar eax,04
  jmp returnqa

SmartCopAI1:
  jmp newmemqa
  nop
returnqa:
registersymbol(SmartCopAI1)


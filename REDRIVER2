aobscanmodule(roadblockPedsLookSort,$process,51 50 C6 46 4E 00) // should be unique
alloc(newmema1,$200)
//35
label(codea1)
label(returna1)

newmema1:

codea1:
  cmp byte ptr [NumPlayers],1
  je doTheLook
  //Get wanted players:
  push eax
  push ecx //player 1 wanted.
  push edx //player 2 wanted.
  movsx eax,[player+48] //check player 1.
  movzx ecx,word ptr [pedestrianFelony]
  cmp eax,-1
  jle @f
  imul eax,260
  movzx ecx,word ptr [eax+car_data+252]
  @@:
  cmp ecx,292
  mov ecx,1
  jae @f
  mov ecx,0
  @@:
  movsx eax,[player+48+74] //check player 2.
  movzx edx,word ptr [pedestrianFelony]
  cmp eax,-1
  jle @f
  imul eax,260
  movzx edx,word ptr [eax+car_data+252]
  @@:
  cmp edx,292
  mov edx,1
  jae @f
  mov edx,0
  @@:

  //Check who to look to:
  cmp ecx,0
  je checkLookP2
  cmp edx,0
  jne lookAtClosest
  jmp popAndDoTheLook
  checkLookP2:
  cmp edx,0
  jne lookAtP2
  jmp lookAtClosest

  makeLookRandom: //canceled logic.
  pop edx
  pop ecx
  pop eax
  mov eax,[esi]
  cmp [esi],0
  jne @f
  mov eax,[esi+4]
  @@:
  push edx
  mov edx,eax
  mov ecx,[esi+14]
  sub ecx,[edx+14]
  mov eax,[esi+1C]
  sub eax,[edx+1C]
  pop edx
  jmp doTheLook

  lookAtClosest:
  push edi
  lea eax,[esi+14]
  push eax
  push player
  call Long2DDistance
  add esp,4*2
  mov edi,eax
  lea eax,[esi+14]
  push eax
  push player+74
  call Long2DDistance
  add esp,4*2
  cmp eax,edi
  pop edi
  jb lookAtP2
  jmp popAndDoTheLook

  lookAtP2:
  pop edx
  pop ecx
  pop eax
  mov ecx,[esi+14]
  sub ecx,[player+74]
  mov eax,[esi+1C]
  sub eax,[player+8+74]
  jmp doTheLook

  popAndDoTheLook:
  pop edx
  pop ecx
  pop eax
  doTheLook:
  push ecx
  push eax
  mov byte ptr [esi+4E],00
  jmp returna1

roadblockPedsLookSort:
  jmp newmema1
  nop
returna1:
registersymbol(roadblockPedsLookSort)


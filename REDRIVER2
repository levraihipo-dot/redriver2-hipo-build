LuaCall(function cycleFullCompact(sender,force) local state = not(compactmenuitem.Caption == 'Compact View Mode'); if force~=nil then state = not force end; compactmenuitem.Caption = state and 'Compact View Mode' or 'Full View Mode'; getMainForm().Splitter1.Visible = state; getMainForm().Panel4.Visible    = state; getMainForm().Panel5.Visible    = state; end; function addCompactMenu() if compactmenualreadyexists then return end; local parent = getMainForm().Menu.Items; compactmenuitem = createMenuItem(parent); parent.add(compactmenuitem); compactmenuitem.Caption = 'Compact View Mode'; compactmenuitem.OnClick = cycleFullCompact; compactmenualreadyexists = 'yes'; end; addCompactMenu(); cycleFullCompact(nil,true))

{$lua}
local memrec = getAddressList().getMemoryRecordByDescription'Update Lists'
memrec.active = true
local memrec = getAddressList().getMemoryRecordByDescription'LUA Code for the Scripts'
memrec.active = true
{$asm}

alloc(CTValues,$60)
registersymbol(CTValues)
CTValues+8:
  db 1

alloc(PlayerPositionAvg,$10)
registersymbol(PlayerPositionAvg)

alloc(NumPursuersPerPlayer,$10)
registersymbol(NumPursuersPerPlayer)
{
  -0: Num Pursuers Player 1.
  -1: Num Pursuers Player 2.
  -2: Player 1 Wanted.
  -3: Player 2 Wanted.
  -4: Player 1 Seen.
  -5: Player 2 Seen.
}

alloc(IndependentFunctions,400)
createthread(IndependentFunctions)
label(CT_FixVegasSky)
registersymbol(CT_FixVegasSky)

IndependentFunctions:
  push 1
  call sleep
  //Custom functions calling thread:
  call WritePlayerPosAvg
  cmp byte ptr [CTValues+10],0
  je @f
  call DoMenuMusic
  @@:
  mov [LeadValues+1C],2 //Fix lead AI crash.
  cmp [CameraCnt],0
  je @f
  cmp [CameraCnt],4
  ja @f
  call CT_FixVegasSky
  @@:
  jmp IndependentFunctions

  //Calls: ----------------------------------------------------
  //Player position average:
  WritePlayerPosAvg:
  cmp byte ptr [NumPlayers],01
  jne Multiplayer

   Singleplayer:
   mov eax,[player]
   mov [PlayerPositionAvg],eax
   mov eax,[player+4]
   mov [PlayerPositionAvg+4],eax
   mov eax,[player+8]
   mov [PlayerPositionAvg+8],eax
   ret

   Multiplayer:
   mov esi,[player]
   add esi,[player+74]
   sar esi,1
   mov [PlayerPositionAvg],esi
   mov esi,[player+4]
   add esi,[player+74+4]
   sar esi,1
   mov [PlayerPositionAvg+4],esi
   mov esi,[player+8]
   add esi,[player+74+8]
   sar esi,1
   mov [PlayerPositionAvg+8],esi
   ret

  //Play menu music:
  DoMenuMusic:
  cmp [gInFrontend],0
  je stopMenuMusic
  mov byte ptr [CTValues+11],0
  cmp [gPlaying],0
  jne @f
  call PrepareXA
  push 0
  push 7
  call PlayXA
  add esp,8
  @@:
  ret
  stopMenuMusic:
  cmp byte ptr [CTValues+11],0
  jne @f
  mov byte ptr [CTValues+11],1
  call UnprepareXA
  call StopXA
  @@:
  ret

  //Fix Vegas dawn and cloudy skies swapped:
  CT_FixVegasSky:
  push eax
  push ecx
  push edx
  push edi
  push esi
  cmp [gInFrontend],0
  jne endVegasSkyFix
  cmp [GameLevel],2
  jne endVegasSkyFix
  cmp byte ptr [NoPlayerControl],0
  jne endVegasSkyFix
  cmp [gTimeOfDay],3
  je endVegasSkyFix
  cmp [gWeather],0
  je @f
  mov esi,[gTimeOfDay]
  mov edi,[gWeather]
  mov [gTimeOfDay],0
  mov [gWeather],0
  call LoadSky
  mov [gTimeOfDay],esi
  mov [gWeather],edi
  jmp endVegasSkyFix
  @@:
  cmp [gTimeOfDay],0
  jne endVegasSkyFix
  mov esi,[gWeather]
  mov [gWeather],2
  call LoadSky
  mov [gWeather],esi
  endVegasSkyFix:
  pop esi
  pop edi
  pop edx
  pop ecx
  pop eax
  ret

[DISABLE]
//LuaCall(cycleFullCompact(nil,false))
KeepMeOn

{$lua}
local memrec = getAddressList().getMemoryRecordByDescription'Update Lists'
memrec.active = off

</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>1123</ID>
          <Description>"Pursuers Can Target Anyone (CUSTOM)"</Description>
          <Options moHideChildren="1" moDeactivateChildrenAsWell="1" moAllowManualCollapseAndExpand="1"/>
          <LastState Activated="1"/>
          <Color>C08000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
alloc(PursueCiv,$10)
registersymbol(PursueCiv)

PursueCiv:
  db 0 //Chase active.
  db -1 //Getaway car ID.
  db 0 //A player is cop.
  db 0 //Immunity previous state.
  dd #20000 //Target damage.

aobscanmodule(CivHitCop,$process,8D 86 E0 00 00 00) // should be unique
alloc(newmem,$500)
//1
//Pursuers will chase civilians when they get hit by them, and the civilian will try to lose them.
label(code)
label(return)

newmem:

code:
  cmp [GameType],01
  jne Proceed
  cmp [CopsAllowed],0
  je Proceed
  cmp byte ptr [PursueCiv],0
  jne Proceed

CheckCollision:
  push eax
  push edi
  push edx

  mov edi,esi
  mov edx,ebx

  //=========================================
  // ONLY TRIGGER IF PLAYER INVOLVED
  //=========================================

  cmp byte ptr [esi+14D],1
  je PlayerIsInvolved

  cmp byte ptr [ebx+14D],1
  je PlayerIsInvolved

  jmp ProceedPop


PlayerIsInvolved:

  // Déterminer qui est le getaway (civilian)
  cmp byte ptr [esi+14D],1
  jne @f
  mov edi,esi
  mov edx,ebx
  jmp ContinueChecks
  @@:
  mov edi,ebx
  mov edx,esi


ContinueChecks:

  //=========================================
  // SAFE CANCEL IF GETAWAY DEAD
  //=========================================

  movsx ecx,byte ptr [edx+14F]
  cmp ecx,-1
  je ProceedPop
  cmp ecx,20
  jae ProceedPop

  imul ecx,260
  mov ax,[PursueCiv+4]
  cmp [ecx+car_data+25A],ax
  jae ProceedPop


  //=========================================

  cmp byte ptr [edx+14D],2 //Civilian only
  jne ProceedPop

  test byte ptr [edx+14E],3
  jne ProceedPop

  mov byte ptr [PursueCiv+2],1

  mov eax,0
  call DoPursuitSpeechMusic

  jmp ActivateChaseStuff


ActivateChaseStuff:

  mov byte ptr [PursueCiv],1

  movsx eax,byte ptr [edx+14F]
  mov [PursueCiv+1],al

  mov [player+43],al

  mov eax,edx
  call ActivateGetaway
  mov word ptr [edx+252],0292

  mov byte ptr [DamageBar+18],1

  push eax
  mov eax,[PursueCiv+4]
  mov [DamageBar+E],ax
  pop eax

  push eax
  mov eax,[gPlayerImmune]
  mov [PursueCiv+3],al
  mov [gPlayerImmune],1
  pop eax


ProceedPop:
  pop edx
  pop edi
  pop eax

  Proceed:
  lea eax,[esi+000000E0]
  jmp return

  ActivateGetaway: //input: eax (cp)
  push eax
  push ecx
  push edx
  movzx ecx,[eax+25A]
  shr ecx,1
  mov [eax+25A],ecx
  push eax
  call InitLead
  add esp,4
  mov [LeadValues],#500
  mov [LeadValues+4],#1000
  mov [LeadValues+8],#428
  mov [LeadValues+C],#21
  mov [LeadValues+10],#885
  mov [LeadValues+14],#10
  mov [LeadValues+18],#487
  mov [LeadValues+1C],#2
  mov [LeadValues+20],#700
  mov [LeadValues+24],#350
  mov [LeadValues+28],#1731
  mov [LeadValues+2C],#30
  mov [LeadValues+30],#320
  mov [LeadValues+34],#7
  mov [LeadValues+38],#225
  mov [LeadValues+3C],#3
  pop edx
  pop ecx
  pop eax
  ret

  DoPursuitSpeechMusic:
  push ecx
  push edx
  cmp eax,0
  jne @f
  push 0
  push B
  call CopSay
  add esp,4*2
  pop edx
  pop ecx
  ret
  @@:
  cmp eax,1
  jne @f
  push 0
  push C
  call CopSay
  add esp,4*2
  @@:
  pop edx
  pop eax
  ret

CivHitCop:
  jmp newmem
  nop
return:
registersymbol(CivHitCop)

//--------------------------------------------------------------------------------

aobscanmodule(ApplyCopTarget,$process,33 C0 66 89 86 6A 01 00 00) // should be unique
alloc(newmema,$580)
//2
//Use unused car_data+14B to apply pursuer target.
label(codea)
label(returna)

newmema:

codea:
  xor eax,eax
  mov [esi+0000016A],ax

  call CheckWhosWanted

  CountPursuers:
  push eax //player car id.
  push ecx //car count.
  push edx //cop count.
  push edi //player offset.
  push esi //car address.
  test [CameraCnt],E
  jne endIt1
  xor edi,edi
  next15:
  mov ecx,-1
  xor edx,edx
  next16:
  inc ecx
  cmp ecx,#20
  jae skip13
  imul esi,ecx,260
  cmp byte ptr [esi+car_data+14D],3
  jne next16
  cmp byte ptr [esi+car_data+163],0
  jne next16
  movsx esi,byte ptr [esi+car_data+14B]
  movsx eax,byte ptr [edi+player+48]
  cmp edi,0
  je @f
  cmp eax,-1
  jne @f
  mov eax,-2 //do p2 on foot
  @@:
  cmp esi,eax
  jne next16
  inc edx
  jmp next16
  skip13:
  xor esi,esi
  cmp edi,0
  je @f
  mov esi,1
  @@:
  mov [esi+NumPursuersPerPlayer],dl
  cmp edi,0
  jne @f
  mov edi,74
  jmp next15
  endIt1:
  pop esi
  pop edi
  pop edx
  pop ecx
  pop eax

  StopPlayerTimer:
  cmp [GameType],4
  jne CheckWanted0
  cmp [CameraCnt],10
  jb CheckWanted0
  cmp byte ptr [NumPursuersPerPlayer],0
  jne @f
  mov byte ptr [Mission+20],5
  @@:
  cmp byte ptr [NumPursuersPerPlayer+1],0
  jne @f
  mov byte ptr [Mission+2C],5
  @@:

  CheckWanted0:
  push ecx
  push edx
  movsx edx,byte ptr [NumPursuersPerPlayer+2]
  movsx ecx,byte ptr [NumPursuersPerPlayer+3]
  add edx,ecx
  cmp edx,2
  pop edx
  pop ecx
  jb DetermineTarget

  DistributePursuersEqually:
  push eax
  mov eax,[numActiveCops]
  cmp [maxCopCars],eax
  pop eax
  jae DetermineTarget
  //Distribute pursuers equally:
  push eax
  push ebx //count.
  push ecx //car count.
  push edx //player car id.
  push edi //player number.
  push esi //car address.
  xor edi,edi
  htnextpl:
  xor ebx,ebx
  mov ecx,-1
  movzx eax,byte ptr [edi+NumPursuersPerPlayer]
  nextcopVeh:
  mov esi,[numActiveCops]
  test esi,1
  je @f
  inc esi //fix odd.
  @@:
  shr esi,1
  cmp esi,eax
  jae finishP1
  inc ecx
  cmp ecx,14
  jae finishP1
  imul esi,ecx,260
  cmp byte ptr [esi+car_data+14D],3
  jne nextcopVeh
  movsx edx,byte ptr [player+48]
  cmp edi,0
  jne @f
  movsx edx,byte ptr [player+48+74]
  cmp edx,-1
  jne @f
  mov edx,-2
  @@:
  cmp [esi+car_data+14B],dl
  je nextcopVeh
  mov [esi+car_data+14B],dl
  dec eax
  jmp nextcopVeh
  finishP1:
  cmp edi,0
  jne @f
  mov edi,1
  jmp htnextpl
  @@:
  pop esi
  pop edi
  pop edx
  pop ecx
  pop ebx
  pop eax

  DetermineTarget:
  push eax
  call GetPursuerTarget
  mov [targetVehicle],eax
  pop eax
  jmp returna

  //Calls:
  UpdateOnFootTarget: //Input: esi (pursuer cp), output: eax(cond.: onfoot cp)
  cmp byte ptr [esi+14B],-1
  jne p2onfootupdate
  cmp byte ptr [player+48],-1
  je @f
  movsx eax,[player+48]
  mov [esi+14B],al
  imul eax,260
  add eax,car_data
  ret
  @@:
  push edi
  mov eax,#20
  imul eax,260
  add eax,car_data
  mov edi,[player]
  mov [eax+14],edi
  mov edi,[player+8]
  mov [eax+1C],edi
  pop edi
  ret
  p2onfootupdate:
  cmp byte ptr [player+48+74],-1
  je @f
  movsx eax,[player+48+74]
  mov [esi+14B],al
  imul eax,260
  add eax,car_data
  ret
  @@:
  push edi
  mov eax,#20
  imul eax,260
  add eax,car_data
  mov edi,[player+74]
  mov [eax+14],edi
  mov edi,[player+8+74]
  mov [eax+1C],edi
  pop edi
  ret

  CheckWhosWanted:
  push ecx
  push edx
  mov edx,[pedestrianFelony]
  cmp byte ptr [player+48],-1
  je @f
  movzx ecx,[player+48]
  imul ecx,260
  movzx edx,word ptr [ecx+car_data+252]
  @@:
  cmp edx,292
  jbe @f
  mov byte ptr [NumPursuersPerPlayer+2],1
  jmp CheckWantedP2
  @@:
  mov byte ptr [NumPursuersPerPlayer+2],0
  CheckWantedP2:
  mov edx,[pedestrianFelony]
  cmp byte ptr [player+48+74],-1
  je @f
  movzx ecx,[player+48+74]
  imul ecx,260
  movzx edx,word ptr [ecx+car_data+252]
  @@:
  cmp edx,292
  jbe @f
  mov byte ptr [NumPursuersPerPlayer+3],1
  jmp here7843
  @@:
  mov byte ptr [NumPursuersPerPlayer+3],0
  here7843:
  pop edx
  pop ecx
  ret

  GetPursuerTarget: //Input: esi (pursuer cp) Output: eax (final target cp)
  push edi
  cmp esi,0
  jne @f
  mov edi,car_data+2F80
  jmp noCustomPath
  @@:
  movsx edi,[esi+14B]
  imul edi,260
  add edi,car_data //Target car pointer.
  cmp byte ptr [esi+14B],-1
  jg @f
  push eax
  call UpdateOnFootTarget
  mov edi,eax
  pop eax
  @@:
  push ecx
  push esi
  push edi
  lea edi,[edi+14]
  push edi
  lea esi,[esi+14]
  push esi
  call Long2DDistance
  add esp,8
  pop edi
  pop esi
  pop ecx
  cmp eax,#20000 //Check distance to target
  jb noCustomPath //If too far fix the AI by doing pursuer target distance average:
  push ecx
  push edx
  push ebx
  mov edx,[edi+14]
  add edx,[esi+14]
  sar edx,1
  mov ecx,eax
  sub ecx,#20000
  mov ebx,#30000
  @@:
  cmp ecx,ebx
  jb @f
  add edx,[esi+14]
  sar edx,1
  sub ecx,ebx
  add ebx,#7500
  jmp @b
  @@:
  mov [car_data+2F80+14],edx
  mov edx,[edi+1C]
  add edx,[esi+1C]
  sar edx,1
  mov ecx,eax
  sub ecx,#20000
  mov ebx,#30000
  @@:
  cmp ecx,ebx
  jb @f
  add edx,[esi+1C]
  sar edx,1
  sub ecx,ebx
  add ebx,#7500
  jmp @b
  @@:
  mov [car_data+2F80+1C],edx
  pop ebx
  pop edx
  pop ecx
  mov edi,car_data+2F80
  noCustomPath:
  mov eax,edi
  pop edi
  ret

ApplyCopTarget:
  jmp newmema
  nop 4
returna:
registersymbol(ApplyCopTarget)

//---------------------------------------------------------------------------------

aobscanmodule(P2PursuerState,$process,8D 45 E4 57 50 8D) // should be unique
alloc(newmemb,$80)
//3
//Check if player 2 is being seen by pursuers.
label(codeb)
label(returnb)

newmemb:

codeb:
  //Activate player 2 data:
  cmp byte ptr [PlayerNum],00
  je @f
  cmp byte ptr [player+46+74],02
  je @f
mov eax,[player+74]
mov [ebp-14],eax
mov eax,[player+4+74]
mov [ebp-10],eax
mov eax,[player+8+74]
mov [ebp-0C],eax

  @@:
  lea eax,[ebp-1C]
  push edi
  push eax
  jmp returnb

P2PursuerState:
  jmp newmemb
returnb:
registersymbol(P2PursuerState)


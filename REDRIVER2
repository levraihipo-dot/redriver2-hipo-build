aobscanmodule(IncRespawnTime,$process,8B 15 ** ** ** ** 0F 44 15 ** ** ** ** 8B 3D ** ** ** **) // should be unique
alloc(newmemv,$100)
//28
label(codev)
label(returnv)

newmemv:

codev:
  mov edx,[gCopRespawnTime]
  cmp [numActiveCops],00
  je NoTimerChange

  push ebx
  push ecx
  xor ebx,ebx
  @@:
  movsx ecx,[ebx+player+48]
  cmp ecx,-1
  je CancelMe1
  imul ecx,260
  cmp word ptr [ecx+car_data+252],293
  jb CancelMe1
  cmp ebx,00
  jne @f
  mov ebx,74
  jmp @b
  @@:
  imul edx,3
  CancelMe1:
  pop ecx
  pop ebx
  jmp returnv

  NoTimerChange:
  cmp byte ptr [Roadblock+28],02
  jmp returnv

IncRespawnTime:
  jmp newmemv
  nop
returnv:
registersymbol(IncRespawnTime)

//--------------------------------------------------------------------------------

aobscanmodule(MaxCopsAdjust,$process,8B 1D ** ** ** ** 83 C4 04 B9 ** ** ** ** 33 D2) // should be unique
alloc(newmemw,$500)
//29
//Increase max cops for p2 as well.
label(codew)
label(returnw)

newmemw:

codew:
  cmp byte ptr [GameType],1
  jne maxCopAdjustCode
  //Checks during civ pursuit: ----
  cmp byte ptr [PursueCiv],0
  je maxCopAdjustCode
  //Reset when car not found:
  cmp byte ptr [PursueCiv+1],-1
  je CancelCivChase
  push esi
  movsx esi,byte ptr [PursueCiv+1]
  imul esi,260
  add esi,car_data
  cmp byte ptr [esi+14D],4
  pop esi
  je DoCivChaseProcess
  CancelCivChase:
  cmp byte ptr [PursueCiv+2],0
  je @f
  push eax
  movzx eax,byte ptr [PursueCiv+3]
  mov [gPlayerImmune],eax
  pop eax
  @@:
  mov [PursueCiv],0000FF00 //Deactivate chase
  jmp maxCopAdjustCode
  DoCivChaseProcess:
  //Increment his felony level:
  push esi
  movsx esi,byte ptr [PursueCiv+1]
  imul esi,260
  add esi,car_data
  cmp word ptr [esi+252],1000
  jge @f
  inc word ptr [esi+252]
  @@:
  pop esi
  //Activate passive cops if player is a cop:
  {push eax
  mov eax,[MissionHeader]
  cmp [eax+6C],0
  pop eax}
  //jne AddCivWaypoint
  push eax
  push ecx
  cmp byte ptr [PursueCiv+2],0
  je CivPursuitEndChecks
  push edi
  mov edi,-1
  @@:
  inc edi
  cmp edi,#20
  jae @f
  imul eax,edi,260
  add eax,car_data
    push eax
    push edx
    mov edx,[MissionHeader]
    movsx eax,byte ptr [eax+13B]
    imul eax,4
    cmp [edx+60+eax],0
    pop edx
    pop eax
    jne @b
  test [eax+14E],1
  je @b
  movsx ecx,byte ptr [PursueCiv+1]
  call ActivateCopWithTarget
  jmp @b
  @@:
  pop ecx
  pop eax
  //Add waypoint:
  AddCivWaypoint:
  push edi
  push ecx
  push esi
  mov edi,[MissionHeader]
  add edi,84
  mov ecx,C
  imul ecx,40
  add edi,ecx
  mov byte ptr [edi],2
  mov [edi+4],40000E01
  mov byte ptr [edi+8],30
  movsx ecx,byte ptr [PursueCiv+1]
  mov [edi+18],ecx
  movsx esi,byte ptr [PursueCiv+1]
  imul esi,260
  add esi,car_data
  mov ecx,[esi+14] //Copy positions.
  mov [edi+C],ecx
  mov ecx,[esi+1C]
  mov [edi+10],ecx
  pop esi
  pop ecx
  pop edi
  CivPursuitEndChecks:
  //Activate damage and force speed:
  push eax
  push esi
  movsx esi,byte ptr [PursueCiv+1]
  imul esi,260
  add esi,car_data
  mov byte ptr [esi+1B4],1 //Activate damage.
  cmp word ptr [esi+250],0
  jle @f
  mov eax,[esi+128]
  movsx eax,word ptr [eax+6A]
  imul eax,1560
  add eax,800
  sar eax,C
  mov [esi+250],ax
  @@:
  pop esi
  pop eax
  //Check getaway distance:
  push eax
  push ecx
  push edx
  push edi
  push esi
  movsx esi,byte ptr [PursueCiv+1]
  imul esi,260
  add esi,car_data
  lea eax,[esi+14]
  push eax
  push player
  call Long2DDistance
  add esp,2*4
  cmp byte ptr [NumPlayers],1
  je measureGetawayDistance
  mov edi,eax
  lea eax,[esi+14]
  push eax
  push player+74
  call Long2DDistance
  add esp,2*4
  cmp edi,eax
  ja @f
  mov eax,edi
  measureGetawayDistance:
  cmp eax,#30000
  pop esi
  pop edi
  pop edx
  pop ecx
  pop eax
  jb @f
  push eax
  mov eax,0
  call deactivateAIChase
  pop eax
  jmp maxCopAdjustCode
  @@:
  //Check if getaway is dead:
  push ecx
  movsx ecx,byte ptr [PursueCiv+1]
  imul ecx,260
  push eax
  mov eax,[PursueCiv+4]
  cmp [ecx+car_data+25A],ax
  pop eax
  pop ecx
  jb @f
  push eax
  mov eax,1
  call deactivateAIChase
  pop eax
  jmp maxCopAdjustCode
  @@:

  maxCopAdjustCode:
  mov ebx,[player+48]
  cmp byte ptr [NumPlayers],1
  je returnw
  push eax
  movsx eax,[player+48+74]
  cmp eax,-1
  je @f
  imul eax,260
  cmp word ptr [eax+car_data+252],292
  jbe @f
  mov ebx,[player+48+74]
  @@:
  pop eax
  jmp returnw

  //Calls:
  deactivateAIChase: //Input: eax (success)
  push eax
  push ebx
  push ecx
  push edx
  //Do cop speech and music:
  cmp byte ptr [PursueCiv+2],0
  je @f
  mov ebx,eax
  inc eax
  call DoPursuitSpeechMusic
  mov byte ptr [player+43],-1 //Remove target.
  mov byte ptr [player+60],0 //Disable siren p1.
  mov byte ptr [player+60+74],0 //Disable siren p2.
  @@:
  //Deactivate the getaway:
  movsx edx,byte ptr [PursueCiv+1]
  imul edx,260
  add edx,car_data
  mov eax,[MaxPlayerDamage]
  mov [edx+25A],ax
  push edx
  push 0
  push edx
  call InitCivState
  add esp,4*2
  pop edx
  mov [edx+249],3
  mov [edx+15C],7
  //Remove waypoint:
  push edi
  push ecx
  mov edi,[MissionHeader]
  add edi,84
  mov ecx,C
  imul ecx,40
  add edi,ecx
  mov [edi+4],0
  pop ecx
  pop edi
  //Deactivate the pursuers:
  mov ecx,-1
  @@:
  inc ecx
  cmp ecx,#20
  jae @f
  imul edx,ecx,260
  add edx,car_data
  movsx eax,byte ptr [PursueCiv+1]
  cmp [edx+14B],al
  jne @b
  push ecx
  push edx
  push 0
  push edx
  call InitCivState
  add esp,4*2
  pop edx
  pop ecx
  or [edx+14E],6
  mov [edx+249],3
  mov [edx+15C],7
  jmp @b
  @@:
  //Restore immunity:
  cmp byte ptr [PursueCiv+2],0
  je @f
  movzx eax,byte ptr [PursueCiv+3]
  mov [gPlayerImmune],eax
  mov word ptr [PursueCiv],FF00 //Deactivate chase
  mov byte ptr [DamageBar+18],0
  @@:
  pop edx
  pop ecx
  pop ebx
  pop eax
  ret

  CheckIfCopVehicle: //Input: eax, Output: eax.
  push ebx
  movzx eax,byte ptr [eax+0000013B]
  mov ebx,[MissionHeader]
  cmp [eax*4+ebx+60],00
  je @f
  mov eax,00 //Not cop car.
  pop ebx
  ret
  @@:
  mov eax,01 //Cop car.
  pop ebx
  ret

  ActivateCopWithTarget: //Input: eax (cop cp), ecx (target ID)
  push edi
  mov edi,eax
  mov [edi+14B],cl //Who to chase (unused address).
  mov word ptr [edi+14D],0003
  mov eax,[gCopDifficultyLevel]
  add eax,2
  mov [edi+14C],al
  dec [numCivCars]
  inc [numActiveCops]
  pop edi
  ret

MaxCopsAdjust:
  jmp newmemw
  nop
returnw:
registersymbol(MaxCopsAdjust)

//--------------------------------------------------------------------------------

aobscanmodule(RoadblockP2,$process,89 5D D4 89 5D E0) // should be unique
alloc(newmemx,$400)
//30
label(codex)
label(returnx)

newmemx:

codex:
  cmp byte ptr [NumPlayers],1
  je OriginalRoadblock

  cmp byte ptr [player+48],-1
  je CheckRBP2
  cmp byte ptr [player+48+74],-1
  je CheckRBP1
  push ecx
  movzx ecx,byte ptr [player+48]
  imul ecx,260
  cmp word ptr [ecx+car_data+252],292 //P1 not wanted.
  pop ecx
  jbe CheckRBP2
  push ecx
  movzx ecx,byte ptr [player+74+48]
  imul ecx,260
  cmp word ptr [ecx+car_data+252],292 //P2 not wanted.
  pop ecx
  jbe CheckRBP1
  //Both wanted check faster:
  push ecx
  push edx
  movzx ecx,byte ptr [player+48]
  movzx edx,byte ptr [player+74+48]
  imul ecx,260
  imul edx,260
  cmp ecx,[ecx+car_data+70]
  mov edx,[edx+car_data+70]
  cmp ecx,edx
  pop edx
  pop ecx
  jb CheckRBP2

  CheckRBP1:
  push eax
  xor eax,eax
  call PlayerRoadblockTooClose
  cmp eax,0
  pop eax
  je OriginalRoadblock
  jmp P2Roadblock

  CheckRBP2:
  push eax
  mov eax,1
  call PlayerRoadblockTooClose
  cmp eax,0
  pop eax
  je P2Roadblock

  OriginalRoadblock:
  add esp,4
  push edi
  mov [ebp-2C],ebx
  mov [ebp-20],ebx
  jmp returnx

  P2Roadblock:
  mov ecx,[player+74+10]
  mov edx,[player+74+8]
  mov [ebp-34],eax
  mov eax,[MissionHeader]
  push ebx
  push esi
  mov esi,[requestCopCar]
  xor ebx,ebx
  mov eax,[eax+6C]
  mov [ebp-3C],eax
  mov eax,[player+74]
  mov [ebp-28],eax
  mov eax,[player+74+4]
  jmp OriginalRoadblock

  PlayerRoadblockTooClose: //input: eax (player), output: eax (yes too close)
  //Prevent roadblock from spawning right in front of a player.
  push ecx
  push edx
  push ebx
  mov ebx,eax
  xor ebx,1
  imul eax,74
  imul ebx,74
  mov edx,[eax+player+10]
  and edx,FFF
  movsx ecx,word ptr [edx*4+rcossin_tbl]
  movsx edx,word ptr [edx*4+rcossin_tbl+2]
  imul ecx,4
  imul edx,4
  add ecx,[eax+player]
  add edx,[eax+player+8]
  mov [roadblockLoc],ecx
  mov [roadblockLoc+8],edx
  lea ebx,[ebx+player]
  push roadblockLoc
  push ebx
  call Long2DDistance
  add esp,8
  mov ebx,eax
  xor eax,eax
  cmp ebx,#12000
  jae @f
  mov eax,1
  @@:
  pop ebx
  pop edx
  pop ecx
  ret

RoadblockP2:
  jmp newmemx
  nop
returnx:
registersymbol(RoadblockP2)


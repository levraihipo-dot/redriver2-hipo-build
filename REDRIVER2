        <CheatEntry>
          <ID>968</ID>
          <Description>"Hood Camera"</Description>
          <Options moHideChildren="1" moDeactivateChildrenAsWell="1" moAllowManualCollapseAndExpand="1"/>
          <LastState Activated="1"/>
          <Color>C08000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]

alloc(hoodCameraPosData,400)
registersymbol(hoodCameraPosData)

hoodCameraPosData: //Vehicles camera height and z position, in model ID order:
  dd 0 0
//Chicago:
  dd #66 #80 //0
  dd #68 #78 //1
  dd #68 #84 //2
  dd #68 #84 //3
  dd #90 #-50 //4
  dd #-1 #0 //8
  dd #80 #-4 //9
  dd #60 #30 //10
  dd #-1 #0 //11
  dd #68 #90 //12
//Havana:
  dd #62 #56 //0
  dd #74 #73 //1
  dd #75 #20 //2
  dd #75 #22 //3
  dd #74 #24 //4
  dd #80 #0 //8
  dd #62 #46 //9
  dd #86 #0 //10
  dd #86 #-40 //11
  dd #58 #-76 //12
//Vegas:
  dd #60 #70 //0
  dd #66 #80 //1
  dd #60 #78 //2
  dd #66 #76 //3
  dd #70 #25 //4
  dd #56 #70 //8
  dd #25 #70 //9
  dd #25 #22 //10
  dd #80 #140 //11
  dd #70 #32 //12
//Rio:
  dd #65 #0 //0
  dd #78 #30 //1
  dd #57 #-2 //2
  dd #55 #15 //3
  dd #60 #32 //4
  dd #-1 #0 //8
  dd #90 #30 //9
  dd #40 #73 //10
  dd #60 #45 //11
  dd #80 #45 //12

aobscanmodule(HoodCam,$process,C7 45 F0 D8 FF FF FF) // should be unique
alloc(newmem,$300)

label(code)
label(return)

newmem:

code:
  push eax
  push ecx
  push edx
  cmp byte ptr [NoPlayerControl],00
  jne CancelProcedure
  call CheckVehicleModel

  Vehicles:
  //Calculate data offset:
  mov edx,[GameLevel]
  cmp byte ptr [CTValues+12],0 //vehicles from other cities
  je @f
  cmp eax,5
  jb @f
  mov edx,[CTValues+13] //city from which.
  @@:
  imul edx,#80
  mov ecx,eax
  cmp ecx,#12
  ja CancelProcedure
  cmp ecx,8
  jl @f
  sub ecx,3
  @@:
  imul ecx,#8
  add edx,ecx
  add edx,8
  cmp [edx+hoodCameraPosData],-1 //check if disabled.
  je CancelProcedure
  //Transfer position data:
  //Do height:
  mov ecx,[edx+hoodCameraPosData]
  cmp byte ptr [NumPlayers],1 //check split screen.
  je @f
  cmp byte ptr [NoPlayerControl],0
  jne @f
  sub ecx,#13
  @@:
  neg ecx
  mov [ebp-10],ecx //Height.
  //Fix look back weird cam pos bug:
  test [paddcamera],0200
  jne CancelProcedure
  push eax
  mov eax,[paddcamera]
  and eax,3
  cmp eax,3
  pop eax
  je CancelProcedure
  //Do x pos:
  push eax
  mov eax,[CurrentPlayerView]
  cmp byte ptr [eax+hoodCameraPosData],0
  je @f
  add [ebp-14],#47 //X position (driver POV mode).
  @@:
  pop eax
  //Do z pos:
  mov ecx,[edx+hoodCameraPosData+4]
  cmp byte ptr [NumPlayers],1 //check split screen.
  je @f
  cmp byte ptr [NoPlayerControl],0
  jne @f
  add ecx,#18
  @@:
  mov eax,[gCameraDefaultScrZ] //adjust for FOV.
  sub eax,#256
  cmp eax,0
  jle @f
  mov eax,0
  @@:
  add ecx,eax
  mov [ebp-0C],ecx //Z position.
  jmp TerminateProcedure

  TerminateProcedure:
  pop edx
  pop ecx
  pop eax
  jmp return

  CancelProcedure:
  pop edx
  pop ecx
  pop eax
  mov [ebp-10],FFFFFFD8
  jmp return

  //Calls:
  CheckVehicleModel: //Input ebx(=car_data+20), Output eax.
  push edx
  push ecx
  movzx eax,byte ptr [ebx+0000011B]
  mov edx,[MissionHeader]
  imul ecx,eax,4
  add edx,60
  add edx,ecx
  mov eax,[edx]
  pop ecx
  pop edx
  ret

HoodCam:
  jmp newmem
  nop 2
return:
registersymbol(HoodCam)

//-----------------------------------------------------------------------

aobscanmodule(DoNotDrawCar,$process,0F 85 A5 00 00 00 8A) // should be unique
alloc(newmema,$100)

label(codea)
label(returna)

newmema:

codea:
  jne DrawCar+132 //Jumps to draw vehicle (Original Code).

  push eax
  push ecx
  push edx

  cmp byte ptr [NoPlayerControl],00
  jne DoNotDrawVehicle
  test [paddcamera],0203
  jne DoNotDrawVehicle
  call CheckVehicleModel2

  //Calculate offset:
  mov edx,[GameLevel]
  cmp byte ptr [CTValues+12],0 //vehicles from other cities
  je @f
  cmp eax,5
  jb @f
  mov edx,[CTValues+13] //city from which.
  @@:
  imul edx,#80
  mov ecx,eax
  cmp ecx,8
  jl @f
  sub ecx,3
  @@:
  imul ecx,#8
  add edx,ecx
  add edx,8
  cmp [edx+hoodCameraPosData],-1 //disabled.
  jne DrawVehicle

  DoNotDrawVehicle:
  pop edx
  pop ecx
  pop eax
  jmp returna

  DrawVehicle:
  pop edx
  pop ecx
  pop eax
  jmp DrawCar+132

  //Calls:
  CheckVehicleModel2: //Input esi, Output eax.
  push edx
  push ecx
  movzx eax,byte ptr [esi+0000013B]
  mov edx,[MissionHeader]
  imul ecx,eax,4
  add edx,60
  add edx,ecx
  mov eax,[edx]
  pop ecx
  pop edx
  ret

DoNotDrawCar:
  jmp newmema
  nop
returna:
registersymbol(DoNotDrawCar)


alloc(newmem,$100)
//Player direction.
label(code)
label(return)

newmem:

code:
  //Hide if player not seen:
  //singleplayer:
  push eax
  movzx eax,byte ptr [NumPlayers]
  add al,[CTValues+7]
  cmp eax,3
  pop eax
  je @f
  cmp [CopsCanSeePlayer],0
  je CancelIndicator
  jmp getP2Rotation
  @@:
  //multiplayer:
  push eax
  mov eax,[CurrentPlayerView]
  cmp byte ptr [eax+NumPursuersPerPlayer+4],0
  pop eax
  je CancelIndicator //pops and return.
  //Get player 2 rotation:
  getP2Rotation:
  cmp [CurrentPlayerView],00
  je @f
  mov eax,[player+10+74]
  @@:
  and eax,00000FFF
  jmp return

  CancelIndicator:
  jmp DrawCopIndicators+E3

DrawCopIndicators1:
  jmp newmem
return:

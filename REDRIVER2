aobscanmodule(SpawnExtraVehicles,$process,B9 26 05 00 00) // should be unique
alloc(newmema,$400)

label(codea)
label(returna)

newmema:

codea:
  SpawnVehicles: //Spawn extra mission vehicles:
  push eax //Values.
  push ebx //Counter.
  push ecx
  push edx
  push edi //Address offset.
  push esi //Vehicle address.
  mov ebx,-1
  NextECar:
  inc ebx
  cmp ebx,7
  ja NoMoreCars
  imul edi,ebx,20
  cmp byte ptr [edi+ExtraVehicles+F],00
  je NextECar
  cmp byte ptr [edi+ExtraVehicles+F],02
  ja NextECar
  movzx eax,[edi+ExtraVehicles+F]
  dec eax
  cmp [gInGameCutsceneActive],eax
  je NextECar
  cmp [GameType],00
  jne @f
  movsx eax,[edi+ExtraVehicles+10] //Check cutscene ID.
  cmp [gInGameCutsceneID],eax
  jne NextECar
  @@:
  //Check proximity to render area:
  push ecx
  push [player+14]
  lea ecx,[edi+ExtraVehicles]
  push ecx
  call Long2DDistance
  add esp,08
  pop ecx
  cmp eax,4000
  ja NextECar
  //If take a ride only spawn it when secret vehicle is loaded:
  cmp [GameType],01
  jne @f
  mov edx,[MissionHeader]
  cmp [edx+70],0C
  jne NextECar
  @@: //Spawn delay:
  cmp byte ptr [edi+ExtraVehicles+F],02
  jb @f
  cmp byte ptr [edi+ExtraVehicles+15],0
  jle @f
  dec byte ptr [edi+ExtraVehicles+15]
  jmp NextECar
  @@:
  inc byte ptr [edi+ExtraVehicles+F]
  push 00
  push 00
  movzx eax,byte ptr [edi+ExtraVehicles+D]
  push eax //Model.
  mov eax,2
  cmp byte ptr [edi+ExtraVehicles+F],03
  jb @f
  movzx eax,[edi+ExtraVehicles+11]
  @@:
  push eax //Control (scripted).
  mov eax,ExtraVehicles
  add eax,edi
  push eax //Position vector.
  movzx eax,[edi+ExtraVehicles+C]
  imul eax,100
  push eax //Rotation.
  mov ecx,8
  @@:
  inc ecx
  cmp ecx,#19
  jae @f
  imul esi,ecx,260
  add esi,car_data
  cmp byte ptr [esi+14D],00
  jne @b
  @@:
  push esi //Car slot.
  call InitCar
  add esp,1C
  mov eax,[edi+ExtraVehicles+E]
  mov [esi+13C],al //Load palette.
  mov eax,74
  imul eax,14
  add eax,player+4A
  mov byte ptr [eax],-8
  mov [esi+00000150],eax //Make car scripted
  or byte ptr [esi+0000014E],0C //Owned by player.
  mov byte ptr [esi+00000249],03 //Engine off.
  mov byte ptr [esi+0000015C],07 //Keep it inactive.
  cmp byte ptr [edi+ExtraVehicles+11],03
  jne @f
  mov word ptr [esi+0000014D],0702 //Make it patrolling pursuer.
  jmp NextECar
  @@:
  cmp byte ptr [edi+ExtraVehicles+11],06
  jne NextECar
  mov word ptr [esi+0000014D],0602 //Make it roadblock cop.
  jmp NextECar
  NoMoreCars:
  pop esi
  pop edi
  pop edx
  pop ecx
  pop ebx
  pop eax

  SavePlayerPosition:
  cmp [gCurrentMissionNumber],07
  je @f
  cmp [gCurrentMissionNumber],0E
  je @f
  cmp [gCurrentMissionNumber],13
  je @f
  jmp LoadPlayer2Damage
  @@:
  push eax
  mov eax,[player+46+74]
  mov [Player2SavedData+12],al //On foot or in vehicle.
  mov eax,[player+74]
  mov [Player2SavedData],eax //X.
  mov eax,[player+4+74]
  mov [Player2SavedData+4],eax //Y.
  mov eax,[player+8+74]
  mov [Player2SavedData+8],eax //Z.

  mov eax,[player+10+74]
  and eax,FFF
  mov [Player2SavedData+E],ax //Rot.
  cmp byte ptr [Player2SavedData+12],02
  pop eax
  je ResumeCode

  push eax
  movzx eax,byte ptr [player+48+74]
  imul eax,260
  add eax,car_data
  movzx eax,word ptr [eax+13B]
  mov [Player2SavedData+D],ah //Palette.

  push edx
  and eax,FF
  mov edx,[MissionHeader]
  imul eax,4
  add edx,60
  add edx,eax
  mov eax,[edx]
  pop edx
  mov [Player2SavedData+C],al //Model.

  movzx eax,byte ptr [player+48+74]
  imul eax,260
  add eax,car_data
  push esi
  mov esi,eax
  movzx eax,word ptr [esi+25A]
  mov [Player2SavedData+10],ax //Damage.
  //Save denting:
  mov eax,[esi+13E]
  mov [Player2SavedData+14],eax
  mov eax,[esi+13E+4]
  mov [Player2SavedData+18],eax
  mov eax,[esi+13E+8]
  mov [Player2SavedData+1C],eax
  pop esi
  pop eax
  jmp ResumeCode

  LoadPlayer2Damage:
  cmp [gCurrentMissionNumber],08
  je @f
  cmp [gCurrentMissionNumber],0F
  je @f
  cmp [gCurrentMissionNumber],14
  je @f
  jmp ResumeCode
  @@:
  cmp [gHaveStoredData],00
  je ResumeCode
  cmp [CameraCnt],2
  ja ResumeCode
  cmp word ptr [Player2SavedData+12],01
  jne ResumeCode
  push eax
  mov eax,[Player2SavedData+10]
  push esi
  movzx esi,byte ptr [player+48+74]
  imul esi,260
  add esi,car_data
  mov [esi+25A],ax //Load damage.
  mov eax,[Player2SavedData+4]
  mov [esi+18],eax //Load Y position.
  //Load denting:
  mov eax,[Player2SavedData+14]
  mov [esi+13E],eax
  mov eax,[Player2SavedData+18]
  mov [esi+13E+4],eax
  mov eax,[Player2SavedData+1C]
  mov [esi+13E+8],eax
  push ecx
  push edx
  push esi
  call DentCar
  add esp,04
  pop edx
  pop ecx
  pop esi
  pop eax

  ResumeCode:
  mov ecx,00000526
  jmp returna

SpawnExtraVehicles:
  jmp newmema
returna:
registersymbol(SpawnExtraVehicles)


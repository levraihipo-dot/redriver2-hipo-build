aobscanmodule(P2FelonyBar,$process,68 ** ** ** ** E8 ** ** ** ** 83 C4 04 68 ** ** ** ** E8 ** ** ** ** 83 C4 04 E8) // should be unique
alloc(newmemg,$180)
//8
//Add player 2 felony bar and sync players felony on mission start.
label(codeg)
label(returng)

newmemg:

codeg:
  //Player 2 Felony Bar:
  cmp byte ptr [NumPlayers],01
  je ContinueIt

  InitializeP2Bar:
  push eax
  push ebx
  mov ebx,-4
  nextBarData:
  add ebx,4
  cmp ebx,18
  ja noMoBar
  mov eax,[ebx+FelonyBar]
  cmp ebx,4
  jne @f
  add eax,6C0000
  @@:
  mov [ebx+Player2FelonyBar],eax
  jmp nextBarData
  noMoBar:
  pop ebx
  pop eax

  UpdateP2Bar:
  push eax
  cmp [gWidescreenOverlayAlign],00 //Align with widescreen HUD.
  je @f
  mov eax,[FelonyBar+4]
  mov [Player2FelonyBar+4],ax
  @@: //Show current player 2 felony state:
  movsx eax,[player+48+74]
  cmp eax,-1
  je @f
  imul eax,260
  mov eax,[eax+car_data+252]
  jmp jfkdl
  @@:
  mov eax,[pedestrianFelony] //Shared with p1 when on foot.
  jfkdl:
  mov [Player2FelonyBar+C],ax //State.
  pop eax
  push Player2FelonyBar
  call DrawPercentageBar
  add esp,04

  ContinueIt:
  cmp byte ptr [NumPlayers],01
  jne @f
  mov [Player2FelonyBar+18],00
  @@:

  push FelonyBar //Original code.
  jmp returng

P2FelonyBar:
  jmp newmemg
returng:
registersymbol(P2FelonyBar)


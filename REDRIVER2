aobscanmodule(FixCarInvisible,$process,33 C2 2B C2 3B C1 0F) // should be unique
alloc(newmemb,$80)

label(codeb)
label(returnb)

newmemb:

codeb:
  xor eax,edx
  sub eax,edx

  push eax
  mov eax,[currentplayerview]
  imul eax,74
  add eax,player+44
  cmp byte ptr [eax],2
  pop eax
  jne @f
  push eax
  push ecx
  mov eax,[esi+14F]
  mov ecx,[currentplayerview]
  imul ecx,74
  add ecx,player+49
  cmp [ecx],al
  pop ecx
  pop eax
  je returnb+6
  @@:

  cmp eax,ecx
  jmp returnb

FixCarInvisible:
  jmp newmemb
  nop
returnb:
registersymbol(FixCarInvisible)


alloc(RBSettings,$10)
registersymbol(RBSettings)
 {-0: Use custom speed.
  -1: (empty).
  -2 w: Minimum distance to rubberband
  -4: How much boost.
  -5 w: Max boost.
  -7: Use cop handling.}

aobscanmodule(PlayerRubberbanding,$process,1D 8B 86 28 01 00 00 0F BF 40) // should be unique
alloc(newmem,$200)

label(code)
label(return)

newmem:

code:
  //Original Code:
  mov eax,[esi+00000128]
  movsx eax,word ptr [eax+6A]

  SetRubberbandPresets:
  cmp byte ptr [RBSettings],00
  jne ApplyRubberbandSettings
  cmp [GameType],3 //Quick chase.
  jne @f
  mov word ptr [RBSettings+2],1000
  mov byte ptr [RBSettings+4],6
  mov word ptr [RBSettings+5],800
  mov word ptr [RBSettings+7],1
  jmp ApplyRubberbandSettings
  @@:
  cmp [GameType],4 //Quick getaway.
  jne @f
  mov word ptr [RBSettings+2],1E00
  mov byte ptr [RBSettings+4],2
  mov word ptr [RBSettings+5],300
  mov word ptr [RBSettings+7],0
  jmp ApplyRubberbandSettings
  @@:
  cmp [GameType],8 //Survival.
  jne @f
  mov word ptr [RBSettings+2],0
  mov byte ptr [RBSettings+4],6
  mov word ptr [RBSettings+5],1000
  mov word ptr [RBSettings+7],1
  jmp ApplyRubberbandSettings
  @@:
  cmp [GameType],B //Cops 'n' Robbers.
  jne @f
  mov word ptr [RBSettings+2],2800
  mov byte ptr [RBSettings+4],3
  mov word ptr [RBSettings+5],300
  mov word ptr [RBSettings+7],1
  jmp ApplyRubberbandSettings
  @@:

  ApplyRubberbandSettings:
  push edi
  push ebx
  cmp byte ptr [esi+14D],01
  jne ContinueCode
  call AssignTargets
  mov ebx,[esi+150] //Check player.
  sub ebx,4A
  cmp byte ptr [ebx+43],-1
  je ContinueCode
  @@:
  call ReduceMaxSpeed
  call GetTargetDistance
  movsx ebx,word ptr [RBSettings+2]
  cmp edi,ebx
  jb ApplyExtraSettings
  sub edi,ebx
  shr edi,4
  movzx ebx,byte ptr [RBSettings+4]
  imul edi,ebx
  movzx ebx,word ptr [RBSettings+5]
  cmp edi,ebx
  jb @f
  mov edi,ebx
  @@:
  add eax,edi

  ApplyExtraSettings:
  push ecx
  mov ecx,[esi+150]
  cmp byte ptr [ecx-7],-1
  je @f
  cmp byte ptr [RBSettings+7],00
  je @f
  mov byte ptr [esi+0000014C],03 //AI cop handling.
  @@:
  pop ecx

  cmp [esi+150],player+BE
  jne ContinueCode
  cmp [GameType],8
  jne ContinueCode
  mov word ptr [esi+0000025A],0 //P2 is invincible.

  ContinueCode:
  pop ebx
  pop edi
  jmp return

  //Calls:
  ReduceMaxSpeed:
  movzx ebx,byte ptr [RBSettings+4]
  shl ebx,4
  push ecx
  movzx ecx,word ptr [RBSettings+2]
  shr ecx,8
  sub ebx,ecx
  pop ecx
  sub eax,ebx
  ret

  GetTargetDistance: //Output: edi.
  push eax
  push ecx
  push edx
  lea ebx,[esi+14]
  mov edi,[esi+150]
  movzx edi,byte ptr [edi-7]
  imul edi,260
  lea edi,[edi+car_data+14]
  push ebx
  push edi
  call Long2DDistance
  add esp,08
  mov edi,eax
  pop edx
  pop ecx
  pop eax

  AssignTargets:
  cmp [GameType],3
  jne @f
  push eax
  movzx eax,byte ptr [player+43]
  mov [player+43+74],al
  pop eax
  ret
  @@:
  cmp [GameType],4
  je @f
  cmp [GameType],8
  je @f
  jmp heirh
  @@:
  push eax
  movsx eax,[player+48]
  mov [player+43+74],al
  pop eax
  ret
  heirh:
  cmp [GameType],B
  jne @f
  push eax
  movsx eax,[player+48+74]
  mov [player+43],al
  pop eax
  @@:
  ret

PlayerRubberbanding+01:
  jmp newmem
  nop 5
return:

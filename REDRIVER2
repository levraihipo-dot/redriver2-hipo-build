</AssemblerScript>
        </CheatEntry>
        <CheatEntry>
          <ID>1008</ID>
          <Description>"Gear Indicator"</Description>
          <Options moHideChildren="1" moDeactivateChildrenAsWell="1" moAllowManualCollapseAndExpand="1"/>
          <LastState Activated="1"/>
          <Color>C08000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]

alloc(GearNumberData,$10)

alloc(GearNumberColors,$10)
registersymbol(GearNumberColors)
{- 00-02: Default color.
 - 03-05: Reverse color.
 - 06-08: Flash color.
}
GearNumberColors:
  db #255 #255 #128
  db #255 #255 #128
  db #255 #0 #0

aobscanmodule(GearNumber,$process,0F 85 ** ** 00 00 80 3D ** ** ** ** 01 56 76 ** 83 3D) // should be unique
alloc(newmem,$180)

label(code)
label(return)

newmem:

code:
  jne DisplayOverlays+219
  push edi
  push esi
  mov edi,player

  PlayerTurn:
  cmp [gDoOverlays],0
  je SkipGearNumber
  cmp [g_FreeCameraEnabled],0
  jne SkipGearNumber
  movsx esi,byte ptr [edi+49]
  cmp esi,-1
  je SkipGearNumber
  imul esi,260
  add esi,car_data

  WriteGearNumber:
  push eax
  cmp [esi+70],00
  jnl @f
  cmp word ptr [esi+00000250],00
  jnl @f
  mov eax,52 //Write reverse.
  jmp KeepR
  @@:
  movzx eax,byte ptr [esi+8A]
  add eax,1
  cmp byte ptr [esi+255],0 //if car burning out show gear 1.
  je @f
  mov eax,1
  @@:
  or al,30
  KeepR:
  mov [GearNumberData],al

  PrintGearNumber:
  push ebx
  push ecx
  push edx
  //Blue:
  mov ecx,[GearNumberColors]
  cmp byte ptr [GearNumberData],52
  jne @f
  mov ecx,[GearNumberColors+3]
  @@:
  call CalculateGearColor
  shr ecx,10
  and ecx,FF
  shr ecx,1
  push ecx
  //Green:
  mov ecx,[GearNumberColors]
  cmp byte ptr [GearNumberData],52
  jne @f
  mov ecx,[GearNumberColors+3]
  @@:
  call CalculateGearColor
  shr ecx,8
  and ecx,FF
  shr ecx,1
  push ecx
  //Red:
  mov ecx,[GearNumberColors]
  cmp byte ptr [GearNumberData],52
  jne @f
  mov ecx,[GearNumberColors+3]
  @@:
  call CalculateGearColor
  and ecx,FF
  shr ecx,1
  push ecx
  call SetTextColour
  mov eax,E0
  cmp byte ptr [NumPlayers],01
  je @f
  cmp byte ptr [NoPlayerControl],00
  jne @f
  cmp edi,player
  jne @f
  mov eax,00
  cmp byte ptr [CTValues+F],0
  jne @f
  cmp [gMultiplayerLevels],0
  jne @f
  mov eax,68
  @@:
  push eax //Y pos.
  mov eax,[gMapXOffset]
  sub eax,2
  push eax //X pos.
  push GearNumberData
  call PrintString
  add esp,18
  pop edx
  pop ecx
  pop ebx
  pop eax
  SkipGearNumber:
  cmp byte ptr [NumPlayers],01
  je @f
  cmp byte ptr [NoPlayerControl],00
  jne @f
  cmp edi,player
  jne @f
  mov edi,player+74
  jmp PlayerTurn
  @@:

  pop esi
  pop edi
  jmp return

  //calls:
  CalculateGearColor:
  push edx
  //Check if correct speed for the gear:
  movzx edx,byte ptr [esi+64] //Wheels in the air.
  add dl,[esi+6C]
  cmp edx,00
  je KeepColors
  cmp [esi+70],00 //Reversing.
  jl KeepColors
  cmp byte ptr [esi+255],0 //Car burning out.
  jne KeepColors
  movzx edx,byte ptr [esi+8A]
  imul edx,2B000 //Too slow.
  cmp [esi+70],edx
  jl @f
  cmp byte ptr [esi+8A],3 //Can't be too fast for 4th gear.
  je KeepColors
  add edx,64000 //Too fast.
  cmp [esi+70],edx
  jl KeepColors
  @@:
  test byte ptr [CameraCnt],04
  je KeepColors
  mov ecx,[GearNumberColors+6]
  KeepColors:
  pop edx
  ret

GearNumber:
  jmp newmem
  nop
return:
registersymbol(GearNumber)


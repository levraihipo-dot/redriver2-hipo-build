SoundEffectPlayListTimer = createTimer()
SoundEffectPlayListTimer.Interval = 500
SoundEffectPlayListTimer.Enabled = false


function getSoundEffectPlayList()
    local SoundEffectPlayList = ""

    local ctValues = getAddressSafe("CTValues")
    if not ctValues then return "" end
    local soundBank = readByte(ctValues + 0x8)
    local soundEffect = readByte(ctValues + 0x9)
    if not soundBank or not soundEffect then return "" end

    local soundSets = {
        [0] = {""},
        [1] = {"Paper hit","Cone hit","Metal hit","Smashable hit","Weak collision","Medium collision","Strong collision","Tire screech","Thunder","Sand terrain","Gravel terrain","Wet terrain","Special siren"},
        [2] = {"Police siren",'"North"','"East"','"South"','"West"',"Red light!","Car heavily damaged","Civilian hit 1","Civilian hit 2","Pursuit announce","Suspect found 1","Suspect found 2","Suspect lost","Megaphone 1","Megaphone 2","Dialogue 1","Dialogue 2","Dialogue 3"},
        [3] = {"Engine 1","Idle engine 1","Horn 1","Engine 2","Idle engine 2","Horn 2","Engine 3","Idle engine 3","Horn 3","Engine 4","Idle engine 4","Horn 4","Engine 5","Idle engine 5","Horn 5","Engine 6","Idle engine 6","Horn 6",},
        [4] = {"Ambiance 1","Ambiance 2","Ambiance 3","Ambiance 4","Car loudspeaker beat"},
        [5] = {"Sound 1","Sound 2","Sound 3","Sound 4","Sound 5"},
        [6] = {"Foot 1","Foot 2","Door open","Door close","Starter motor",'"Hey!"'},
    }

    for i=0,18 do
      if soundSets[soundBank][i] then
        local textToAdd = soundSets[soundBank][i]
        SoundEffectPlayList = SoundEffectPlayList .. i-1 .. ":" .. textToAdd .. "\n"
      end
    end

    return SoundEffectPlayList
end

SoundEffectPlayListTimer.OnTimer = function(timer)
    local record = getAddressList().getMemoryRecordByDescription("SoundEffectPlayList")
    if record then
        record.DropDownList.Text = getSoundEffectPlayList()
        record.DisplayAsDropDownListItem = true
        record.DropDownDescriptionOnly = true
    end
end

SoundEffectPlayListTimer.Enabled = true



      <Description>"Improved Vehicle Selection"</Description>
      <Options moHideChildren="1" moDeactivateChildrenAsWell="1"/>
      <LastState/>
      <Color>C08000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]

alloc(CarSlot,$30)
registersymbol(CarSlot)
CarSlot:
  dd 1 //car 1
  dd 2 //car 2
  dd 3 //car 3
  dd 0 //cop
  dd -1 //spec
  db 0 //customize

aobscanmodule(MPVehSelectFix,$process,83 C2 04 83 FA 08) // should be unique
alloc(newmemy,$300)

label(codey)
label(returny)

newmemy:

codey:
  push eax
  push edx
  cmp [GameType],0
  je skipTheCode
  cmp byte ptr [CarSlot+14],0
  jne ignoreDefault
  defaultBasicCars:
  cmp [GameLevel],2
  jne @f
  cmp [GameType],4
  je @f
  cmp [GameType],5
  je @f
  cmp [GameType],6
  je @f
  cmp [GameType],8
  je @f
  cmp [GameType],#11
  je @f
  cmp [GameType],#12
  je @f
  mov [CarSlot],1
  mov [CarSlot+4],3
  mov [CarSlot+8],4
  mov [CarSlot+C],0
  mov [CarSlot+10],-1
  jmp endBasicDefinition
  @@:
  mov [CarSlot],1
  mov [CarSlot+4],2
  mov [CarSlot+8],3
  mov [CarSlot+C],0
  mov [CarSlot+10],-1
  ignoreDefault:

  endBasicDefinition:
  mov eax,[wantedCar]
  mov edx,0
  call updateCarList
  cmp byte ptr [NumPlayers],1
  je @f
  mov eax,[wantedCar+4]
  mov edx,1
  call updateCarList
  @@:

  ApplyTheCars:
  mov edx,[MissionHeader]
  mov eax,[CarSlot]
  mov [edx+60],eax
  mov eax,[CarSlot+4]
  mov [edx+60+4],eax
  mov eax,[CarSlot+8]
  mov [edx+60+8],eax
  mov eax,[CarSlot+C]
  mov [edx+60+C],eax
  mov eax,[CarSlot+10]
  cmp eax,-1
  je @f
  mov [edx+60+10],eax
  @@:

  skipTheCode:
  pop edx
  pop eax
  add edx,04 //Original code.
  cmp edx,08
  jmp returny

  //calls:
  updateCarList: //input: eax (wantedCar), edx (player id), output: eax (yes or no)
  push ecx
  push ebx
  //cmp eax,5
  //jae specialCars
  cmp eax,0
  je copCar
  jmp basicCars
  copCar:
  cmp [CarSlot],0
  je listUpdated
  cmp [CarSlot+4],0
  je listUpdated
  cmp [CarSlot+C],0
  je listUpdated
  cmp [CarSlot+10],0
  je listUpdated
  mov [CarSlot+8],eax
  jmp listUpdated
  basicCars:
  cmp eax,5
  ja specialCars
  xor ebx,ebx //has car.
  mov ecx,-1
  @@:
  inc ecx
  cmp ecx,4
  ja @f
  cmp eax,[CarSlot+ecx*4]
  jne @b
  mov ebx,1
  jmp @b
  @@:
  cmp ebx,0
  jne @f
  call CheckPlayer1Car2Slot
  mov [CarSlot+edx*4],eax
  @@:
    jmp listUpdated
  specialCars:
  //cmp eax,5
  //jb listUpdated
  cmp edx,0
  jne @f
  mov [CarSlot+10],eax
  jmp listUpdated
  @@:
  cmp edx,1
  jne listUpdated
  cmp [wantedCar],5
  jae @f
  mov [CarSlot+10],eax
  jmp listUpdated
  @@:
  cmp [CarSlot+10],eax
  je listUpdated
  mov [CarSlot+8],eax
  listUpdated:
  pop ebx
  pop ecx
  ret

  CheckPlayer1Car2Slot:
  push eax
  cmp edx,0
  je @f
  mov eax,[wantedCar]
  cmp [CarSlot+4],eax
  jne @f
  mov [CarSlot],eax
  @@:
  pop eax
  ret

MPVehSelectFix:
  jmp newmemy
  nop
returny:
registersymbol(MPVehSelectFix)

CarSelectScreen+3E:
  nop 6
//Secret vehicle in mp.

CarSelectScreen+F0:
  nop 6
//Other vehicles on mp.

[DISABLE]

MPVehSelectFix:
  db 83 C2 04 83 FA 08

unregistersymbol(MPVehSelectFix)
dealloc(newmemy)

unregistersymbol(CarSlot)
dealloc(CarSlot)

CarSelectScreen+3E:
  jne +95

CarSelectScreen+F0:
  jne +C4

PingInCivCar+566:
  dd 02020202

LoadLevelSFX+45:
  db 01

{
// ORIGINAL CODE - INJECTION POINT: REDRIVER2.LoadMission+A0F

REDRIVER2.LoadMission+9EF: 83 F9 04                    - cmp ecx,04
REDRIVER2.LoadMission+9F2: 7E E5                       - jle REDRIVER2.LoadMission+9D9
REDRIVER2.LoadMission+9F4: A1 64 7A DB 00              - mov eax,[REDRIVER2.MissionHeader]
REDRIVER2.LoadMission+9F9: 89 48 70                    - mov [eax+70],ecx
REDRIVER2.LoadMission+9FC: EB 07                       - jmp REDRIVER2.LoadMission+A05
REDRIVER2.LoadMission+9FE: 8D 7B 01                    - lea edi,[ebx+01]
REDRIVER2.LoadMission+A01: 85 F6                       - test esi,esi
REDRIVER2.LoadMission+A03: 74 0A                       - je REDRIVER2.LoadMission+A0F
REDRIVER2.LoadMission+A05: 8B 82 68 7A DB 00           - mov eax,[edx+REDRIVER2.PlayerStartInfo]
REDRIVER2.LoadMission+A0B: C6 40 02 00                 - mov byte ptr [eax+02],00
// ---------- INJECTING HERE ----------
REDRIVER2.LoadMission+A0F: 83 C2 04                    - add edx,04
// ---------- DONE INJECTING  ----------
REDRIVER2.LoadMission+A12: 83 FA 08                    - cmp edx,08
REDRIVER2.LoadMission+A15: 0F 8C 75 FF FF FF           - jl REDRIVER2.LoadMission+990
REDRIVER2.LoadMission+A1B: 83 3D 50 7A DB 00 0C        - cmp dword ptr [REDRIVER2.GameType],0C
REDRIVER2.LoadMission+A22: 75 05                       - jne REDRIVER2.LoadMission+A29
REDRIVER2.LoadMission+A24: E8 87 EB FF FF              - call REDRIVER2.ActivateNextFlag
REDRIVER2.LoadMission+A29: A1 54 7B DB 00              - mov eax,[REDRIVER2.MissionScript]
REDRIVER2.LoadMission+A2E: A3 48 79 DB 00              - mov [REDRIVER2.MissionThreads+8],eax
REDRIVER2.LoadMission+A33: A1 44 79 DB 00              - mov eax,[REDRIVER2.MissionThreads+4]
REDRIVER2.LoadMission+A38: 66 C7 05 40 79 DB 00 01 00  - mov word ptr [REDRIVER2.MissionThreads],0001
REDRIVER2.LoadMission+A41: A3 4C 79 DB 00              - mov [REDRIVER2.MissionThreads+C],eax
}
</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>1655</ID>
          <Description>"Choose Traffic Vehicles"</Description>
          <Options moHideChildren="1"/>
          <LastState/>
          <Color>C08000</Color>

alloc(CarEffectsData,$10)
registersymbol(CarEffectsData)
{
  - 0-4: Toggles.
  - 8-B: Vehicles ID.
}
CarEffectsData:
  db 1 1 0 0 1

aobscanmodule(AllCarsEffects,$process,57 05 ** ** ** ** 50 E8) // should be unique
alloc(newmem,$300)

label(code)
label(return)

newmem:

code:
  cmp [CameraCnt],1
  ja @f
  mov [player+5C],-1
  mov [player+5C+74*1],-1
  mov [player+5C+74*2],-1
  mov [player+5C+74*3],-1
  @@:
  push eax
  push ebx
  push ecx
  push esi
  push edi
  mov ebx,1
  mov ecx,-1
  nextControlType:
  mov edi,-1
  nextCar:
  inc edi
  cmp edi,#20
  jae noMoreCars
  imul esi,edi,260
  add esi,car_data
  cmp [esi+14D],bl
  jne nextCar
  checkIfPlayer:
  cmp bl,1
  jne checkIfCutscene
  cmp [esi+150],player+4A
  jne @f
  cmp byte ptr [CarEffectsData],0
  jne nextCar
  jmp addCarEffects
  @@:
  cmp [gInGameCutsceneActive],0
  jne nextCar
  cmp byte ptr [CarEffectsData+1],0
  jne nextCar
  jmp addCarEffects
  checkIfCutscene:
  cmp bl,7
  jne checkIfAIGetaway
  cmp byte ptr [esi+15C],7
  je nextCar
  jmp addCarEffects
  checkIfAIGetaway:
  cmp bl,4
  jne checkIfPursuer
  cmp byte ptr [esi+150],8
  je nextCar
  jmp addCarEffects
  checkIfPursuer:
  cmp bl,3
  jne @f
  cmp byte ptr [esi+163],0
  jne nextCar
  addCarEffects:
  inc ecx
    movsx eax,byte ptr [esi+14F]
    mov [ecx+CarEffectsData+8],al
  push ecx
  push ecx
  push esi
  call CheckCarEffects
  push esi
  call SwirlLeaves
  add esp,4*3
  pop ecx
  cmp ecx,3
  jae noMoreSlots
  jmp nextCar
  noMoreCars:
  cmp ebx,1
  jne @f
  mov ebx,7
  cmp byte ptr [CarEffectsData+2],0
  jne @f
  jmp nextControlType
  @@:
  cmp ebx,7
  jne @f
  mov ebx,4
  cmp byte ptr [CarEffectsData+3],0
  jne @f
  jmp nextControlType
  @@:
  cmp ebx,4
  jne @f
  mov ebx,3
  cmp byte ptr [CarEffectsData+4],0
  jne @f
  jmp nextControlType
  StopInactiveSlots:
  inc ecx
  cmp ecx,3
  ja @f
  push edi
  push esi
  push ecx
  imul esi,ecx,74
  add esi,player
  movsx edi,byte ptr [esi+5C]
  push edi
  call StopChannel
  push edi
  call UnlockChannel
  add esp,4*2
  mov word ptr [esi+5C],-1
  pop ecx
  pop esi
  pop edi
  jmp @b
  @@:
  noMoreSlots:
  pop edi
  pop esi
  pop ecx
  pop ebx
  pop eax
  mov esi,#20
  jmp return+26

AllCarsEffects:
  jmp newmem
  nop
return:
registersymbol(AllCarsEffects)


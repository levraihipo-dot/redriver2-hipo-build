TimerSecondsListTimer = createTimer()
TimerSecondsListTimer.Interval = 100
TimerSecondsListTimer.Enabled = false


function getTimerSecondsList()
    local TimerSecondsList = ""
    local Mission = getAddressSafe("Mission")

    if not Mission or readInteger(Mission) == 0 then Mission = 0 end

    for i=0,1 do
      Mission = Mission + 0xC * i
      local timerRawTime = readInteger(Mission + 0x24)
      if not timerRawTime then timerRawTime = 0 end
      timerMinute = math.floor(timerRawTime / 180000) % 60
      timerSecond = math.floor(timerRawTime / 3000) % 60
      TimerSecondsList = TimerSecondsList .. timerRawTime .. ":" .. timerMinute .. "min" .. timerSecond .. "s\n"
    end

    return TimerSecondsList
end

TimerSecondsListTimer.OnTimer = function(timer)
    local record = getAddressList().getMemoryRecordByDescription("TimerSecondsList")
    if record then
        record.DropDownList.Text = getTimerSecondsList()
        record.DisplayAsDropDownListItem = true
    end
end

TimerSecondsListTimer.Enabled = true



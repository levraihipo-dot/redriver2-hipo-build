        <CheatEntry>
          <ID>978</ID>
          <Description>"Speedometer"</Description>
          <Options moHideChildren="1" moDeactivateChildrenAsWell="1" moAllowManualCollapseAndExpand="1"/>
          <LastState Activated="1"/>
          <Color>C08000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]

alloc(SpeedometerData,$20) //0 - B: Text, C: mph switch, E: p1 above speed limit, F: p2 above speed limit, 10: showing text.
registersymbol(SpeedometerData)

alloc(SpeedoFormat,$10)

alloc(SpeedoColors,$10)
registersymbol(SpeedoColors)
{- 00-02: forward color.
 - 03-05: reverse color.
 - 06-08: stationary color.
 - 09-0B: text color.
 - 0C-0E: flash color.
}
SpeedoColors:
  db #255 #255 #255
  db #255 #128 #128
  db #255 #255 #255
  db #128 #128 #128
  db #255 #0 #0

aobscanmodule(Speedometer,$process,33 C5 89 45 FC 83 3D * * * * 00 74 55) // should be unique
alloc(newmem,$380)

label(code)
label(return)

newmem:

code:
  cmp [gInGameCutsceneActive],00
  jne OriginalCode
  cmp [gInGameCutsceneDelay],00
  jne OriginalCode
  cmp [g_FreeCameraEnabled],0
  jne OriginalCode
  cmp [gShowMap],00
  jne OriginalCode
  cmp [gDoOverlays],0
  je OriginalCode
  push esi
  push edi
  mov edi,player

  PlayerTurn:
  mov byte ptr [SpeedometerData+10],0
  movsx esi,byte ptr [edi+49]
  cmp esi,-1
  je SkipSpeedometer
  imul esi,260
  add esi,car_data

  SpeedProcessing:
  push eax
  push ebx
  push edx
  mov ebx,#6161 //Wheel speed and km/h ratio.
  cmp byte ptr [SpeedometerData+C],00
  je @f
   imul ebx,#1609 //Convert to mph.
   mov eax,ebx
   mov ebx,#1000
   cdq
   idiv ebx
   mov ebx,eax
  @@:
  mov eax,[esi+70]
  cmp eax,00
  jnl @f
  neg eax
  @@:
  cdq
  idiv ebx
  pop edx
  pop ebx

  //Text when rear wheels are not touching the ground:
  cmp byte ptr [esi+64],00 //Right wheel.
  jne HexValueToString
  cmp byte ptr [esi+6C],00 //Left wheel.
  jne HexValueToString
  mov byte ptr [SpeedometerData+10],1
  cmp word ptr [esi+08],63
  jle @f
  mov [SpeedometerData],"    "
  mov [SpeedometerData+4],"--"
  jmp PrintProcess
  @@:
  mov [SpeedometerData],"Flip"
  mov [SpeedometerData+4],"ped"00
  jmp PrintProcess

  HexValueToString:
  cmp eax,#1000
  jb @f
  mov [SpeedometerData],"So f"
  mov [SpeedometerData+4],"ast!"
  mov byte ptr [SpeedometerData+10],1
  jmp PrintProcess
  @@:
  mov word ptr [SpeedoFormat],"%d"
  cmp byte ptr [SpeedometerData+C],00
  jne @f
  mov [SpeedoFormat+02],"km/h"
  mov [SpeedoFormat+06],0
  jmp Push1
  @@:
  mov [SpeedoFormat+02],"mph"00
  mov [SpeedoFormat+06],0
  Push1:
  push eax
  push SpeedoFormat
  push SpeedometerData
  call sprintf
  add esp,C

  PrintProcess:
  push ebx
  push ecx
  push edx
  textColor: //Apply colors:
  cmp byte ptr [SpeedometerData+10],0
  je @f
  mov edx,[SpeedoColors+9]
  jmp SetSpeedoColor
  @@:
  cmp [esi+70],0
  jle @f
  mov edx,[SpeedoColors]
  jmp SetSpeedoColor
  @@:
  je @f
  mov edx,[SpeedoColors+3]
  jmp SetSpeedoColor
  @@:
  mov edx,[SpeedoColors+6]
  SetSpeedoColor:
  mov ecx,edx
  shr ecx,10
  mov ebx,[SpeedoColors+C]
  shr ebx,10
  call CalculateFlashColor
  push ecx //Blue.
  mov ecx,edx
  shr ecx,8
  mov ebx,[SpeedoColors+C]
  shr ebx,8
  call CalculateFlashColor
  push ecx //Green.
  mov ecx,edx
  mov ebx,[SpeedoColors+C]
  call CalculateFlashColor
  push ecx //Red.
  Jmp2:
  call SetTextColour
  mov eax,E0
  cmp byte ptr [NumPlayers],01
  je @f
  cmp byte ptr [NoPlayerControl],00
  jne @f
  cmp edi,player
  jne @f
  mov eax,00
  cmp byte ptr [CTValues+F],0
  jne @f
  cmp [gMultiplayerLevels],0
  jne @f
  mov eax,68
  @@:
  push eax //y pos.
  mov eax,[gMapXOffset]
  add eax,C
  push eax //X pos.
  push SpeedometerData
  call PrintString
  add esp,18
  pop edx
  pop ecx
  pop ebx
  pop eax
  SkipSpeedometer:
  cmp byte ptr [NumPlayers],01
  je @f
  cmp byte ptr [NoPlayerControl],00
  jne @f
  cmp edi,player
  jne @f
  mov edi,player+74
  jmp PlayerTurn
  @@:
  pop edi
  pop esi

  OriginalCode:
  xor eax,ebp
  mov [ebp-04],eax
  jmp return

  //Calls:
  CalculateFlashColor: //Input: ecx (original color), ebx (flash color), output: ecx.
  push eax
  push edx
  and ecx,FF
  and ebx,FF
  shr ecx,1
  shr ebx,1
  cmp [CopsAllowed],00
  je noFlash
  cmp [gPlayerImmune],00
  jne noFlash
  cmp byte ptr [SpeedometerData+10],0
  jne noFlash
  xor edx,edx
  cmp byte ptr [CTValues+7],0
  jne @f
  cmp edi,player //Do not flash player 2's if chase anyone disabled.
  jne noFlash
  @@:
  cmp edi,player
  je @f
  mov edx,1
  @@:
  cmp byte ptr [edx+SpeedometerData+E],00
  je noFlash
  cmp [esi+252],293
  ja noFlash
  mov eax,[CameraCnt]
  and eax,F
  cmp eax,8
  jb @f
  neg eax
  and eax,F
  @@:
  cmp eax,8
  jb @f
  mov eax,7
  @@:
  sub ecx,ebx
  shr ecx,3
  imul ecx,eax
  add ecx,ebx
  and ecx,FF
  noFlash:
  pop edx
  pop eax
  ret

Speedometer:
  jmp newmem
return:
registersymbol(Speedometer)


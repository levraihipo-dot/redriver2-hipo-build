REDRIVER2.State_GameStart+1A7: A3 D0 98 DE 00        - mov [REDRIVER2.gCurrentMissionNumber],eax
REDRIVER2.State_GameStart+1AC: E8 7F EF 02 00        - call REDRIVER2.SetState
REDRIVER2.State_GameStart+1B1: 83 C4 08              - add esp,08
REDRIVER2.State_GameStart+1B4: C3                    - ret
}
</AssemblerScript>

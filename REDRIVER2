aobscanmodule(PursuerLeadLights,$process,C6 05 ** ** ** ** 00 8A 86 4D 01 00 00) // should be unique
alloc(newmem,$100)

label(code)
label(return)

newmem:

code:
  mov byte ptr [TransparentObject],00

  mov al,[esi+0000014D]

  CopsAndLeadLights:
  cmp al,02 //Non-player pursuers and getaways show brake and reverse lights.
  jna return
  mov edi,[esi+70]
  cmp byte ptr [esi+64],00 //Right rear wheel.
  jne DrawCar+612 //Brake light for players "test edi,edi".
  cmp byte ptr [esi+6C],00 //Left rear wheel.
  jne DrawCar+612
  jmp DrawCar+65A //If rear wheels are in the air jump to night lights.

PursuerLeadLights:
  jmp newmem
  nop 2
return:

aobscanmodule(P2Immune,$process,33 C9 66 89 08) // should be unique
alloc(newmemt,$80)
//26
label(codet)
label(returnt)

newmemt:

codet:
  //Player 2 immune too:
  push ecx
  cmp byte ptr [NumPlayers],01
  je @f
  cmp byte ptr [gPlayerImmune],00
  je @f
  movsx ecx,byte ptr [player+48+74]
  cmp ecx,-1
  je @f
  imul ecx,260
  mov word ptr [ecx+car_data+252],00
  @@:
  pop ecx

  xor ecx,ecx //Original code.
  mov [eax],cx
  jmp returnt

P2Immune:
  jmp newmemt
returnt:
registersymbol(P2Immune)


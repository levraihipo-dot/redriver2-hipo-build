aobscanmodule(UpdateTargetPlayer,$process,74 ** 56 8B 75 08 6A 28) // should be unique
alloc(newmemc,$500)
//4
//Cops chase who comitted the felony.
//If both players are wanted they'll chase the first one they see.
//Each player gets as many pursuers as the pursuer limit.
label(codec)
label(returnc)

newmemc:

codec:
  mov esi,[ebp+08]

  cmp byte ptr [NumPlayers],01
  je UpdateTargetNoFelony
  cmp [player+46],2
  je UpdateTargetNoFelony

  call CheckWhosWanted

  CheckWanted:
  push eax
  push ecx
  movsx eax,byte ptr [NumPursuersPerPlayer+2]
  movsx ecx,byte ptr [NumPursuersPerPlayer+3]
  add eax,ecx
  cmp eax,2
  pop ecx
  pop eax
  jae UpdateTargetProximity

  UpdateTargetNoFelony: //If not both are wanted chase whoever's been caught breaking the law.
  push ebx
  movzx ebx,byte ptr [PlayerNum]
  cmp byte ptr [NumPlayers],01
  je @f
  xor ebx,1 //Fix stupid nonsense.
  @@:
  imul ebx,74
  push eax
  movsx eax,byte ptr [ebx+player+48]
  cmp ebx,0
  je @f
  cmp eax,-1
  jne @f
  mov eax,-2 //do p2 on foot.
  @@:
  mov [esi+14B],al
  pop eax
  pop ebx
  jmp outtaHere

  UpdateTargetProximity:
  push eax
  push ebx
  push ecx
  push edx
  lea ecx,[esi+14]
  push player
  push ecx
  call Long2DDistance
  add esp,8
  mov ebx,eax
  lea ecx,[esi+14]
  push player+74
  push ecx
  call Long2DDistance
  add esp,8
  cmp eax,ebx
  jb @f
  movsx ecx,[player+48]
  mov [esi+14B],cl
  pop edx
  pop ecx
  pop ebx
  pop eax
  jmp outtaHere
  @@:
  movsx ecx,[player+48+74]
  cmp ecx,-1
  jne @f
  mov ecx,-2 //do p2 on foot.
  @@:
  mov [esi+14B],cl
  pop edx
  pop ecx
  pop ebx
  pop eax
  jmp outtaHere

  outtaHere:
  push 28
  jmp returnc

UpdateTargetPlayer+03:
  jmp newmemc
returnc:
registersymbol(UpdateTargetPlayer)

//----------------------------------------------------------------------------------

aobscanmodule(NoteFelony1,$process,A0 ** ** ** ** 56 57 84 C0 79 ** BE ** ** ** ** EB) // should be unique
alloc(newmemd,$180)
//5
//Write felony to player 2 as well.
label(coded)
label(returnd)

newmemd:

coded:
  FixPursuitMusic:
  push eax
  push ecx
  push edx
  //Always pursuit theme active:
  cmp byte ptr [CTValues+6],0
  je @f
  mov eax,1
  jmp applyMusicType
  @@:
  //Check if pursuit is active:
  cmp [CopsAllowed],0
  je cancelMusicFix
  //General checks:
  xor eax,eax
  cmp [player_position_known],0
  jle PlayerCopCheckForMusic
  cmp [NumActiveCops],0
  je @f
  cmp word ptr [NumPursuersPerPlayer],0
  je @f
  mov eax,1
  @@:
  cmp [NumRoadblockCars],0
  je @f
  mov eax,1
  @@:
  //Player is a cop:
  PlayerCopCheckForMusic:
  cmp byte ptr [PursueCiv],0
  je @f
  cmp byte ptr [PursueCiv+2],0
  je @f
  mov eax,1
  @@:
  //Apply:
  applyMusicType:
  cmp [copmusic],eax
  je @f
  push eax
  call FunkUpDaBGMTunez
  add esp,4
  cancelMusicFix:
  pop edx
  pop ecx
  pop eax

  writeP2Felony:
  push ebx
  movzx ebx,[PlayerNum]
  imul ebx,74
  mov al,[ebx+player+48]
  pop ebx
  jmp returnd

NoteFelony1:
  jmp newmemd
returnd:
registersymbol(NoteFelony1)

//----------------------------------------------------------------------------------

alloc(FelonyTimers,$10)
 {-0 w: P1 wrong way timer.
  -2 w: P2 wrong way timer.
  -4 w: P1 burnout timer.
  -6 w: P2 burnout timer.
  -8 w: P1 seen.
  -A w: P2 seen.}

aobscanmodule(PlayerFelonies,$process,0F 84 A2 02 00 00 A0) // should be unique
alloc(newmeme,380)
//6
//Player 2 felonies.
label(codee)
label(returne)

newmeme:

codee:
  cmp byte ptr [NumPlayers],01
  je CheckAllPlayersFelonies

  CheckFelonyTimers:
  push eax //Values.
  push ebx //Timer offset.
  push ecx //Address offset.
  push edi //Player offset.
  movzx edi,byte ptr [PlayerNum]
  xor edi,1
  shl edi,1
  xor ecx,ecx
  OtherTimer:
  mov ebx,ecx
  shr ebx,1
  //Write if seen:
  mov eax,[CopsCanSeePlayer]
  mov [edi+FelonyTimers+8],ax
  push ecx
  mov ecx,edi
  shr ecx,1
  xor ecx,1
  mov [ecx+NumPursuersPerPlayer+4],al
  pop ecx
  //Increment timers:
  cmp word ptr [ecx+felonyData+20],01
  jne @f
  add word ptr [ebx+edi+FelonyTimers],2 //Increment 2x.
  @@:
  cmp word ptr [ecx+felonyData+20],00
  jne @f
  mov word ptr [ebx+edi+FelonyTimers],0 //Reset timer.
  @@:
  //Timer limits:
  mov eax,[ecx+felonyData+22]
  cmp [ebx+edi+FelonyTimers],ax
  jb @f
  movzx eax,byte ptr [PlayerNum]
  xor eax,1
  imul eax,74
  movsx eax,byte ptr [eax+player+48]
  cmp eax,-1
  je @f
  imul eax,260
  cmp word ptr [eax+car_data+252],293
  jae @f
  xor edi,2
  cmp word ptr [edi+FelonyTimers+8],00
  je @f
  cmp [numActiveCops],0
  jne @f
  mov word ptr [eax+car_data+252],293 //Note felony artificially :).
  @@:
  cmp ecx,00
  jne @f
  mov ecx,8
  jmp OtherTimer
  @@:
  pop edi
  pop ecx
  pop ebx
  pop eax

  CheckAllPlayersFelonies:
  push ebx
  movzx ebx,[PlayerNum]
  imul ebx,74
  cmp byte ptr [ebx+player+46],02
  je @f
  mov al,[ebx+player+48]
  pop ebx
  jmp returne
  @@:
  pop ebx

  pop edi
  pop esi
  pop ebx
  mov esp,ebp
  pop ebp
  ret

PlayerFelonies:
  jmp newmeme
  nop 6
returne:
registersymbol(PlayerFelonies)


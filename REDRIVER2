alloc(flashingCopInd,$10)
registersymbol(flashingCopInd)

alloc(CarPointer,$10)

aobscanmodule(WriteCurrentCar,$process,F7 F9 50 69 45 F4 0A 01 00 00) // should be unique
alloc(newmem,$100)

label(code)
label(return)

newmem:

code:
  idiv ecx
  push eax
  imul eax,[ebp-0C],0000010A
   mov [CarPointer],esi
  jmp return

WriteCurrentCar:
  jmp newmem
  nop 5
return:
registersymbol(WriteCurrentCar)

//--------------------------------------------------------------------------------

aobscanmodule(FlashCopIndicators,$process,88 43 09 88 43 0A F3) // should be unique
alloc(newmema,$400)

label(codea)
label(returna)

newmema:

codea:
  push edx
  push ecx
  push eax
  mov eax,[CurrentPlayerView]
  cmp byte ptr [eax+flashingCopInd],0
  pop eax
  jne CopFlash1

  //Check type:
  push eax
  mov eax,[CarPointer]
  sub eax,163
  call CheckIfCopVehicle
  cmp eax,00
  pop eax
  jne CopFlash1
  push eax
  mov eax,[CarPointer]
  sub eax,163
  call CheckIfServiceVehicle
  cmp eax,00
  pop eax
  jne ServiceFlash1
  jmp GangsterFlash1

  CopFlash1:
  //Set color:
  mov ecx,[CameraCnt]
  shl ecx,5
  add ecx,10
  test ecx,100
  je @f
  neg ecx
  @@:
  movsx edx,cl
  neg edx
  and ecx,FF
  and edx,FF
  imul edx,[ebp-4]
  shr edx,8
  mov [ebx+08],dl //Red
  mov edx,[ebp-4]
  shr edx,3
  mov [ebx+09],dl //Green
  imul ecx,[ebp-4]
  shr ecx,8
  mov [ebx+0A],cl //Blue
  pop ecx
  pop edx
  jmp returna

  GangsterFlash1:
  mov ecx,[CameraCnt]
  shl ecx,6
  add ecx,10
  test ecx,100
  jne @f
  neg ecx
  @@:
  and ecx,FF
  imul ecx,[ebp-4]
  shr ecx,8
  mov [ebx+08],cl
  mov edx,[ebp-4]
  shr edx,3
  mov [ebx+09],dl
  mov [ebx+0A],dl
  pop ecx
  pop edx
  jmp returna

  ServiceFlash1:
  mov ecx,[CameraCnt]
  shl ecx,6
  add ecx,10
  test ecx,100
  jne @f
  neg ecx
  @@:
  and ecx,FF
  mov edx,[ebp-4]
  shr edx,3
  mov [ebx+08],dl
  mov [ebx+09],dl
  imul ecx,[ebp-4]
  shr ecx,8
  mov [ebx+0A],cl
  pop ecx
  pop edx
  jmp returna

  //Calls:
  CheckIfCopVehicle: //Input: eax, Output: eax.
  push ebx
  movzx eax,byte ptr [eax+0000013B]
  mov ebx,[MissionHeader]
  cmp [eax*4+ebx+60],00
  je @f
  mov eax,00 //Not cop car.
  pop ebx
  ret
  @@:
  mov eax,01 //Cop car.
  pop ebx
  ret

  CheckIfServiceVehicle:
  push ebx
  push ecx
  mov ecx,[GameLevel]
  cmp byte ptr [CTValues+12],0
  je @f
  mov ecx,[CTValues+13]
  @@:
  cmp ecx,1
  je NotService1
  cmp ecx,1
  jb @f
  dec ecx
  @@:
  add ecx,8
  movzx eax,byte ptr [eax+0000013B]
  mov ebx,[MissionHeader]
  cmp [eax*4+ebx+60],ecx
  je ImService1
  NotService1:
  mov eax,00 //Not service.
  pop ecx
  pop ebx
  ret
  ImService1:
  mov eax,01 //Service.
  pop ecx
  pop ebx
  ret

FlashCopIndicators:
  jmp newmema
  nop
returna:
registersymbol(FlashCopIndicators)

[DISABLE]


  cmp byte ptr [NumPlayers],01
  je yioeo

  @@:
  cmp [eax],dx
  jg returnn+2
  movsx eax,[player+48+74]
  cmp eax,-1
  jne @f
  xor eax,eax
  cmp ax,dx
  jmp returnn
  @@:
  imul eax,260
  add eax,car_data+252
  cmp [eax],dx
  jg returnn+2
  jmp returnn
  yioeo:
  cmp [eax],dx
  jmp returnn

PursuitStartFix:
  jmp newmemn
  nop 3
returnn:
registersymbol(PursuitStartFix)


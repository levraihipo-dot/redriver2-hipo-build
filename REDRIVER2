        <CheatEntry>
          <ID>959</ID>
          <Description>"Horn (Down+R1) and Lights Only (Up+R1) in Police/Service Vehicles"</Description>
          <LastState Activated="1"/>
          <Color>C08000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]

aobscanmodule(PlayerEmergencyHorn,$process,F6 C3 08 74 ** 6B C7 74) // should be unique
alloc(newmem,$140)

label(code)
label(return)

newmem:

code:
  //Original Code:
  test bl,08
  je ProcessCarPad+148

  PlayerHorn:
  test bh,40 //D-pad down.
  je SirenRestore
  push eax
  imul eax,edi,74
  cmp byte ptr [eax+player+60],00
  je @f
  imul eax,edi,3
  add eax,2
  imul eax,74
  xor [eax+channels+58],1F40
  pop eax
  jmp ProcessCarPad+190
  @@:
  xor byte ptr [eax+player+62],01
  pop eax
  jmp ProcessCarPad+190

  SirenRestore:
  //Check if needed to restore:
  push eax
  imul eax,edi,3
  add eax,2
  imul eax,74
  mov [eax+channels+58],1F40
  cmp byte ptr [eax+channels+72],02
  pop eax
  jne JustLights
  //Restoring process:
  push eax
  push ecx
  push edx
  push -01 //proximity.
  push 00001000 //pitch
  call CheckIfCopVehicle
  mov edx,02
  mov ecx,00
  cmp eax,00
  jne @f
  mov edx,01 //Fire truck or Ambulance.
  mov ecx,0C
  @@:
  push ecx //sample.
  push edx //bank.
  mov eax,02
  cmp edi,00
  je @f
  mov eax,05
  @@:
  push eax //channel.
  call CompleteSoundSetup
  add esp,14
  pop edx
  pop ecx
  pop eax
  //Start lights without siren (Up+RB):
  JustLights:
  test bh,10 //D-pad up.
  je return
  push eax
  imul eax,edi,3
  add eax,2
  imul eax,74
  mov [eax+channels+58],00
  pop eax
  jmp return

  //Calls:
  CheckIfCopVehicle: //Input: esi, Output: eax.
  push ebx
  movzx eax,byte ptr [esi+0000013B]
  mov ebx,[MissionHeader]
  cmp [eax*4+ebx+60],00
  je @f
  mov eax,00 //Not cop car.
  pop ebx
  ret
  @@:
  mov eax,01 //Cop car.
  pop ebx
  ret

PlayerEmergencyHorn:
  jmp newmem
return:
registersymbol(PlayerEmergencyHorn)


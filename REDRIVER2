              <CheatEntries>
                <CheatEntry>
                  <ID>1613</ID>
                  <Description>"VehicleModelList Update"</Description>
                  <LastState Activated="1"/>
                  <Color>0080FF</Color>
                  <VariableType>Auto Assembler Script</VariableType>
                  <AssemblerScript>{$lua}

[ENABLE]
-- Define global variables so we can clean them up in [DISABLE]
if vehicleListTimer then
  vehicleListTimer.destroy()
end

vehicleListTimer = createTimer()
vehicleListTimer.Interval = 500
vehicleListTimer.Enabled = false

function getVehicleModelList()
    local vehicleList = ""
    local gameLevelAddr = getAddressSafe("GameLevel")
    local ctValuesAddr = getAddressSafe("CTValues")

    if not gameLevelAddr or not ctValuesAddr then return "" end

    local gameLevel = readInteger(gameLevelAddr)
    local altMode = readBytes(ctValuesAddr + 0x12)
    local altSet = readInteger(ctValuesAddr + 0x13, 1)

    local baseSets = {
        [0] = {"Chicago Police", "Oldsmobile 442", "Monarch", "Delta", "Chicago Van"},
        [1] = {"Havana Police", "Kingsway", "Bel Air", "Styleline", "Havana Pickup"},
        [2] = {"Vegas Police", "Challenger", "Lincoln", "Cougar", "Vegas Pickup"},
        [3] = {"Rio Police", "Mercedes", "Thunderbird", "Corcel", "Ram Pickup"},
    }

    local extras = {
        [0] = {"Chicago Fire", "Armored Truck", "School Bus", "Big Rig", "Secret Chicago Car"},
        [1] = {"Flatbed", "Havana Truck", "Havana Bus", "Kombi", "Secret Havana Car"},
        [2] = {"White Limo", "Ambulance", "Vegas Truck", "SUV", "Secret Vegas Car"},
        [3] = {"Rio Bus", "C-Series Pickup", "Rio Fire", "Black Limo", "Secret Rio Truck"},
    }

    local altExtras = extras[altSet]
    local selectedBase = baseSets[gameLevel] or {}
    local selectedExtras = (altMode == 1 and altExtras) or (extras[gameLevel] or {})

    vehicleList = vehicleList .. "-1:None\n"
    for i=0,4 do
        vehicleList = vehicleList .. i .. ":" .. (selectedBase[i+1] or "Unknown") .. "\n"
    end
    for i=8,12 do
        vehicleList = vehicleList .. i .. ":" .. (selectedExtras[i-7] or "Unknown") .. "\n"
    end
    vehicleList = vehicleList .. "13:Extra Vehicle\n"


    return vehicleList
end

-- Timer logic to update dropdown dynamically
vehicleListTimer.OnTimer = function(timer)
    local record = getAddressList().getMemoryRecordByDescription("VehicleModelList")
    if record then
        record.DropDownList.Text = getVehicleModelList()
        record.DisplayAsDropDownListItem = true
        record.DropDownDescriptionOnly = true
    end
end

vehicleListTimer.Enabled = true

[DISABLE]

-- Stop and destroy timer
if vehicleListTimer then
    vehicleListTimer.Enabled = false
    vehicleListTimer.destroy()
    vehicleListTimer = nil
end

--Optional: Clear dropdown text
local record = getAddressList().getMemoryRecordByDescription("VehicleModelList")
if record then
    record.DropDownList.Text = ""
end

</AssemblerScript>
                </CheatEntry>
                <CheatEntry>
                  <ID>1680</ID>
                  <Description>"SpecialVehicleModelList Update"</Description>
                  <LastState Activated="1"/>
                  <Color>0080FF</Color>
                  <VariableType>Auto Assembler Script</VariableType>
                  <AssemblerScript>{$lua}

[ENABLE]
-- Define global variables so we can clean them up in [DISABLE]
if specialVehicleListTimer then
  specialVehicleListTimer.destroy()
end

specialVehicleListTimer = createTimer()
specialVehicleListTimer.Interval = 500
specialVehicleListTimer.Enabled = false

function getSpecialVehicleModelList()
    local specialVehicleList = ""
    local gameLevelAddr = getAddressSafe("GameLevel")
    local ctValuesAddr = getAddressSafe("CTValues")
    local alterCityAddr = getAddressSafe("DesiredCityLevel")

    if not gameLevelAddr or not ctValuesAddr then return "" end

    local gameLevel = readInteger(gameLevelAddr)
    local altMode = readBytes(ctValuesAddr + 0x12)
    local altSet = readByte(alterCityAddr)

    local extras = {
        [0] = {"Chicago Fire", "Armored Truck", "School Bus", "Big Rig", "Secret Chicago Car"},
        [1] = {"Flatbed", "Havana Truck", "Havana Bus", "Kombi", "Secret Havana Car"},
        [2] = {"White Limo", "Ambulance", "Vegas Truck", "SUV", "Secret Vegas Car"},
        [3] = {"Rio Bus", "C-Series Pickup", "Rio Fire", "Black Limo", "Secret Rio Truck"},
    }

    local altExtras = extras[altSet]
    local selectedExtras = (altMode == 1 and altExtras) or (extras[gameLevel] or {})

    specialVehicleList = specialVehicleList .. "-1:None\n0:None\n"
    for i=8,12 do
        specialVehicleList = specialVehicleList .. i .. ":" .. (selectedExtras[i-7] or "Unknown") .. "\n"
    end
    specialVehicleList = specialVehicleList .. "13:Extra Vehicle\n"


    return specialVehicleList
end

-- Timer logic to update dropdown dynamically
specialVehicleListTimer.OnTimer = function(timer)
    local record = getAddressList().getMemoryRecordByDescription("SpecialVehicleModelList")
    if record then
        record.DropDownList.Text = getSpecialVehicleModelList()
        record.DisplayAsDropDownListItem = true
        record.DropDownDescriptionOnly = true
    end
end

specialVehicleListTimer.Enabled = true


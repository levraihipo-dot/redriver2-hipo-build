aobscanmodule(PursuerLimitPerPlayer,$process,89 55 F8 3B C2) // should be unique
alloc(newmems,$100)
//25
//Spawns extra pursuers when both players.
label(codes)
label(returns)

newmems:

codes:
  cmp byte ptr [NumPlayers],01
  je ElOriginal

  cmp [numActiveCops],00
  je ElOriginal

  push ebx
  push ecx
  xor ebx,ebx
  @@:
  movsx ecx,[ebx+player+48]
  cmp ecx,-1
  je CancelMe
  imul ecx,260
  cmp word ptr [ecx+car_data+252],293
  jb CancelMe
  cmp ebx,00
  jne @f
  mov ebx,74
  jmp @b
  @@:
  shl edx,1
  CancelMe:
  pop ecx
  pop ebx

  ElOriginal:
  mov [ebp-08],edx
  cmp eax,edx
  jmp returns

PursuerLimitPerPlayer:
  jmp newmems
returns:
registersymbol(PursuerLimitPerPlayer)

